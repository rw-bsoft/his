<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="phis.application.dpc.schemas.DPC_FixedAgreement" table="DPC_FixedAgreement">
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="personName" type="java.lang.String" length="30" />
		<property name="sex" type="java.lang.String" length="1"/>
		<property name="age" type="java.lang.Integer" length="1"/>
		<property name="diagnosis" type="java.lang.String" length="200"/>
		<property name="doctorId" type="java.lang.String" length="18"/>
		<property name="createUser" type="java.lang.String" length="20"/>
		<property name="createUnit" type="java.lang.String" length="20"/>
		<property name="createDate" type="timestamp"/>
		<property name="lastModifyUser" type="java.lang.String" length="20"/>
		<property name="lastModifyUnit"  type="java.lang.String" length="20"/>
		<property name="lastModifyDate" type="timestamp"/>
		<property name="operationDate" type="timestamp"/>
		<property name="proposed" type="java.lang.String" length="200"/>
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SCM_ServiceItems" table="SCM_ServiceItems">
		<id name="itemCode" type="java.lang.String">
			<column name="itemCode"/>
			<generator class="assigned" />
		</id>
		<property name="parentCode" type="java.lang.String" length="20" />
		<property name="itemName" type="java.lang.String" length="50" />
		<property name="pyCode" type="java.lang.String" length="50" />
		<property name="serviceTimes" type="java.lang.Integer" length="3" />
		<property name="price" type="java.lang.Double" length="6" />
		<property name="discount" type="java.lang.Double" length="6" />
		<property name="realPrice" type="java.lang.Double" length="6" />
		<property name="hisZFBL" type="java.lang.Double" length="6" />
		<property name="intendedPopulation" type="java.lang.String" length="1" />
		<property name="startUsingDate" type="date"  />
		<property name="itemNature" type="java.lang.String" length="1" />
		<property name="itemType" type="java.lang.String" length="1" />
		<property name="isBottom" type="java.lang.String" length="1" />
		<property name="serviceTable" type="java.lang.String" length="200" />
		<property name="serviceFields" type="java.lang.String" length="2000" />
		<property name="moduleAppId" type="java.lang.String" length="200" />
		<property name="isOneWeekWork" type="java.lang.String" length="1" />
		<property name="remark" type="java.lang.String" length="2000" />
		<property name="intro" type="java.lang.String" length="2000" />
		<property name="fyxh" type="java.lang.String" length="32" />
		<property name="ztmc" type="java.lang.String" length="50" />
	</class>
</hibernate-mapping>
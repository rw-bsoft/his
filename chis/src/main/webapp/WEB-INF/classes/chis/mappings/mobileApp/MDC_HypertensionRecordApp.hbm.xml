<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mobileApp.schemas.MDC_HypertensionRecordApp"
		table="MDC_HypertensionRecord">
		<id name="phrId" type="java.lang.String">
			<column name="phrId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
       	<property name="eateHabit"  type="java.lang.String" length="64"  />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp"/>
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="status" type="java.lang.String" length="1" />
		<!-- 需求新加 add by yuhua 2009 10 30-->	
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
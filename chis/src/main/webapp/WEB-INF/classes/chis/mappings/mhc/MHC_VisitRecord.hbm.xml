<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="chis.application.mhc.schemas.MHC_VisitRecord" table="MHC_VisitRecord">
    <id name="visitId" type="java.lang.String">
      <column name="visitId" />
      <generator class="assigned" />
    </id>
    <property name="pregnantId" type="java.lang.String" length="30" />
    <property name="empiId" type="java.lang.String" length="32" />
    <property name="checkWeek" type="java.lang.String" length="2" />
    <property name="selfFeelSymptom" type="java.lang.String"
      length="500" />
    <property name="quickeningTimes" type="java.lang.Integer" />
    <property name="weight" type="java.lang.Double" />
    <property name="weightFlag" type="java.lang.String" length="1" />
    <property name="dbp" type="java.lang.Integer" />
    <property name="sbp" type="java.lang.Integer" />
    <property name="bpFlag" type="java.lang.String" length="1" />
    <property name="heightFundusUterus" type="java.lang.Double" />
    <property name="heightFundusUterusFlag" type="java.lang.String"
      length="1" />
    <property name="abdomenCircumFerence" type="java.lang.Double" />
    <property name="fetalPosition" type="java.lang.String"
      length="3" />
    <property name="fetalPositionFlag" type="java.lang.String"
      length="1" />
    <property name="fetalHeartRate" type="java.lang.Integer" />
    <property name="fetalHeartFlag" type="java.lang.String"
      length="1" />
    <property name="presentPosition" type="java.lang.String"
      length="10" />
    <property name="xianjie" type="java.lang.String" length="10" />
    <property name="edemaStatus" type="java.lang.String"
      length="10" />
    <property name="haemoglobin" type="java.lang.String"
      length="10" />
    <property name="albuminuria" type="java.lang.String"
      length="10" />
    <property name="bloodSugar" type="java.lang.String" length="10" />
    <property name="otherExam" type="java.lang.String" length="100" />
    <property name="bultrasonicDiagnosis" type="java.lang.String"
      length="100" />
    <property name="amnioticFluidDepth" type="java.lang.String"
      length="10" />
    <property name="afi" type="java.lang.String" length="10" />
    <property name="bultrasonicDate" type="date" />
    <property name="bultrasonicId" type="java.lang.String"
      length="30" />
    <property name="highRiskReason" type="java.lang.String"
      length="64" />
    <property name="highRiskScore" type="java.lang.Integer" />
    <property name="highRiskLevel" type="java.lang.String"
      length="10" />
    <property name="category" type="java.lang.String" length="2" />
    <property name="suggestion" type="java.lang.String"
      length="100" />
    <property name="nextDate" type="date" />
    <property name="visitDate" type="date" />
    <property name="doctorId" type="java.lang.String" length="20" />
    <property name="hospitalCode" type="java.lang.String"
      length="20" />
    <property name="gestationMode" type="java.lang.String" length="20" />
    <property name="endDate" type="date" />
    <property name="visitMode" type="java.lang.String" length="1" />
    <property name="ifLost" type="java.lang.String" length="1" />
    <property name="lostReason" type="java.lang.String"
      length="200" />
    <property name="visitResult" type="java.lang.String" length="1" />
    <property name="exceptionDesc" type="java.lang.String"
      length="500" />
    <property name="otherHighReasons" type="java.lang.String"
      length="500" />
    <property name="guide" type="java.lang.String" length="45" />
    <property name="otherGuide" type="java.lang.String" length="500" />
    <property name="referral" type="java.lang.String" length="1" />
    <property name="referralReason" type="java.lang.String" length="500" />
    <property name="referralUnit" type="java.lang.String" length="100" />
    <property name="createUnit" type="java.lang.String" length="20"/>
    <property name="createUser" type="java.lang.String" length="20"/>
    <property name="createDate" type="timestamp"/>
    <property name="lastModifyUnit" type="java.lang.String" length="20"/>
    <property name="lastModifyDate" type="timestamp" />
    <property name="lastModifyUser" type="java.lang.String"
      length="20" />
  </class>
</hibernate-mapping>
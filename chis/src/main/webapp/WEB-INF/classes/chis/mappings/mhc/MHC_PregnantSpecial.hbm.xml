<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mhc.schemas.MHC_PregnantSpecial"
		table="MHC_PregnantSpecial">
		<id name="specialId" type="java.lang.String">
			<column name="specialId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="pregnantId" type="java.lang.String" length="30" />
		<property name="gestationalWeeks" type="java.lang.String" />
		<property name="problems" type="java.lang.String" length="200" />
		<property name="transactDesc" type="java.lang.String"
			length="100" />
		<property name="transactDate" type="date" />
		<property name="occurDate" type="date" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputDate" type="timestamp" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20"/>
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
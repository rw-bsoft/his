<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.scm.schemas.SCM_ServicePackage" table="SCM_ServicePackage">
		<id name="SPID" type="java.lang.String">
			<column name="SPID"/>
			<generator class="assigned" />
		</id>
		<property name="packageName" type="java.lang.String" length="50" />
		<property name="manaUtil" type="java.lang.String" length="20" />
		<property name="spPrice" type="java.lang.Double" length="6" />
		<property name="spDiscount" type="java.lang.Double" length="6" />
		<property name="spRealPrice" type="java.lang.Double" length="6" />
		<property name="startUsingDate" type="date"  />
		<property name="validEndDate" type="date"  />
		<property name="packageIntendedPopulation" type="java.lang.String" length="5" /> 
		<property name="intendedPopulation" type="java.lang.String" length="20" />
		<property name="intro" type="java.lang.String" length="2000" />
		<property name="remark" type="java.lang.String" length="2000" /> 
		<property name="LOGOFF" type="java.lang.Integer" length="1"/>
		<property name="isUsePrice" type="java.lang.Integer" length="1"/>
		<property name="SFXM" type="java.lang.Long" length="18" />
		<property name="sortNumberSP" type="java.lang.Integer" length="3" />
		<property name="isGhsfjm" type="java.lang.Integer" length="1"/>
		<property name="type" type="java.lang.String" length="100" /> 

	</class>
</hibernate-mapping>
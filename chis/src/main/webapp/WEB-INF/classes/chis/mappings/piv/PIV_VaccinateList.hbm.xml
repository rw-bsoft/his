<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.piv.schemas.PIV_VaccinateList" table="PIV_VACCINATELIST">
			<id name="recordId" type="java.lang.String">
            	<column name="recordId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="phrId" type="java.lang.String" length="30" />
        	<property name="immuneType" type="java.lang.String" length="1" />
        	<property name="vaccineCode" type="java.lang.String" length="4" />
        	<property name="vaccineName" type="java.lang.String" length="50" />
        	<property name="times" type="java.lang.String" length="10" />
        	<property name="planDate" type="date"  />
        	<property name="vaccinateDate" type="date"  />
        	<property name="vaccineBN" type="java.lang.String" length="20" />
        	<property name="vaccineManuCode" type="java.lang.String" length="2" />
        	<property name="vaccineManu" type="java.lang.String" length="50" />
        	<property name="vaccinateUser" type="java.lang.String" length="30" />
        	<property name="vaccinateUnit" type="java.lang.String" length="70" />
        	<property name="reactionCode" type="java.lang.String" length="2" />
        	<property name="reactionDate" type="date"  />
        	<property name="reactionTreat" type="java.lang.String" length="1000" />
        	<property name="vaccinatePlace" type="java.lang.String" length="100"/>
</class>
</hibernate-mapping>
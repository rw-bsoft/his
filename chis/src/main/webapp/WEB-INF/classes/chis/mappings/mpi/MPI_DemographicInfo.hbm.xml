<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mpi.schemas.MPI_DemographicInfo" table="MPI_DemographicInfo">
		<id name="empiId" type="java.lang.String" length="32">
			<column name="empiId" />
			<generator class="assigned" />
		</id>
		<property name="personName"      			type="java.lang.String" length="20"/>
		<property name="phoneNumber"      			type="java.lang.String" length="20"/>
		<property name="photo"      				type="java.lang.String" length="100"/>
		<property name="idCard" 					type="java.lang.String" length="20"/>	
		<property name="zipCode" 					type="java.lang.String" length="6"/>
		<property name="address" 					type="java.lang.String" length="100"/>
		<property name="contact"	 				type="java.lang.String" length="20" />
		<property name="contactPhone"				type="java.lang.String" length="20" />
		<property name="sexCode"           	  		type="java.lang.String" length="1"/>
		<property name="cardtype"      				type="java.lang.String" length="10"/>
		<property name="registeredPermanent" 		type="java.lang.String" length="1"/>
		<property name="bloodTypeCode"      		type="java.lang.String" length="1"/>
		<property name="homePlace"           		type="java.lang.String" length="100"/>
		<property name="mobileNumber"      		  	type="java.lang.String" length="20"/>
		<property name="email"            		  	type="java.lang.String" length="30"/>
		<property name="birthday"         			type="date"/>
		<property name="nationalityCode"  	  		type="java.lang.String" length="3"/>
		<property name="nationCode"     	   		type="java.lang.String" length="2"/>
		<property name="rhBloodCode"      			type="java.lang.String" length="1"/>
		<property name="maritalStatusCode" 		  	type="java.lang.String" length="2"/>
		<property name="startWorkDate"      	  	type="date"/>
		<property name="workCode"           		type="java.lang.String" length="3"/>
		<property name="educationCode"      		type="java.lang.String" length="2"/>
		<property name="insuranceCode"       		type="java.lang.String" length="20"/>
		<property name="createUnit"         		type="java.lang.String" length="16"/>
		<property name="createUser"        	  		type="java.lang.String" length="20"/>
		<property name="createTime"           		type="timestamp"/>
		<property name="lastModifyUnit"     		type="java.lang.String" length="16"/>
		<property name="lastModifyTime"    	  		type="timestamp"/>
		<property name="lastModifyUser"    	  		type="java.lang.String" length="20"/>
		<property name="status"             		type="java.lang.String" length="1"/>
		<property name="workPlace"   				type="string" 			length="50" />
		<property name="insuranceType"				type="string" 			length="100"/>
		<property name="versionNumber"				type="string" 			length="32"/>
		<property name="insuranceText"			type="string" 			length="50"/>
		<property name="definePhrid"			type="string" 			length="60"/>
		<property name="zlls"			type="string" 			length="1"/>
        <property name="ylzh"			type="string" 			length="60"/>
        <property name="isnh"			type="string" 			length="1"/>
		<property name="ownerName"				    type="string" 			length="20"/>
	    <property name="familyId"				    type="string" 			length="30"/>
	    <property name="signFlag"				    type="string" 			length="1"/>
	    <property name="sceBeginDate"				type="date"/>
	    <property name="sceEndDate"				    type="date"/>
	    <property name="crowdType"				    type="string" 			length="20"/>
	    <property name="childCard"				    type="string" 			length="20"/>
	    <property name="contactRelation"			type="java.lang.String" length="100" />
	</class>
</hibernate-mapping>
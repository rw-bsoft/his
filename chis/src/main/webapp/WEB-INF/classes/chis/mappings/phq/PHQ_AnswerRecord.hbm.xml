<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.phq.schemas.PHQ_AnswerRecord" table="PHQ_AnswerRecord">
			<id name="answerRecordId" type="java.lang.String">
            	<column name="answerRecordId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="gcId" type="java.lang.String" length="16" />
        	<property name="masterplateId" type="java.lang.String" length="16" />
        	<property name="fieldId" type="java.lang.String" length="16" />
        	<property name="fieldName" type="java.lang.String" length="30" />
        	<property name="fieldValue" type="java.lang.String" length="200" />
</class>
</hibernate-mapping>
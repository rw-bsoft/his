<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.tr.schemas.MDC_TumourConfirmed" table="MDC_TumourConfirmed">
			<id name="TCID" type="java.lang.String">
            	<column name="TCID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="phrId" type="java.lang.String" length="32" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="highRiskType" type="java.lang.String" length="2" />
        	<property name="highRiskSource" type="java.lang.String" length="1" />
        	<property name="confirmedSource" type="java.lang.String" length="1" />
        	<property name="cancerCase" type="java.lang.String" length="1" />
        	<property name="confirmedGist" type="java.lang.String" length="50" />
        	<property name="confirmedDate" type="date"  />
        	<property name="confirmedHospital" type="java.lang.String" length="50" />
        	<property name="cancerStage" type="java.lang.String" length="1" />
        	<property name="T" type="java.lang.String" length="10" />
        	<property name="N" type="java.lang.String" length="10" />
        	<property name="M" type="java.lang.String" length="10" />
        	<!-- 
        	<property name="reviewDate" type="date"  />
        	<property name="reviewSpecialist" type="java.lang.String" length="100" />
        	 -->
        	<property name="year" type="java.lang.Integer" length="4" />
        	<property name="notification" type="java.lang.String" length="1" />
        	<property name="nature" type="java.lang.String" length="1" />
        	<property name="manaDoctorId" type="java.lang.String" length="20" />
        	<property name="manaUnitId" type="java.lang.String" length="20" />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="cancellationReason" type="java.lang.String" length="1" />
        	<property name="cancellationUser" type="java.lang.String" length="20" />
        	<property name="cancellationDate" type="timestamp"  />
        	<property name="cancellationUnit" type="java.lang.String" length="20" />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
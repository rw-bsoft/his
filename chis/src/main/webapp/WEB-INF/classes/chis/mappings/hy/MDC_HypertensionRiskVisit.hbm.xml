<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hy.schemas.MDC_HypertensionRiskVisit"
		table="MDC_HypertensionRiskVisit">
		<id name="visitId" type="java.lang.String">
			<column name="visitId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="riskId" type="java.lang.String" length="16" />
		<property name="constriction" type="java.lang.Integer" length="8" />
		<property name="diastolic" type="java.lang.Integer" length="8" />
		<property name="riskiness" type="java.lang.String" length="64" />
		<property name="waistLine" type="java.lang.Double" length="8" />
		<property name="height" type="java.lang.Double" length="8" />
		<property name="weight" type="java.lang.Double" length="8" />
		<property name="targetWeight" type="java.lang.Double" length="8" />
		<property name="bmi" type="java.lang.Double" length="8" />
		
		<property name="obeyDoctor" type="java.lang.String" length="20" />
		<property name="healthTeach" type="java.lang.String" length="20" />
		<property name="visitWay" type="java.lang.String" length="1" />
		<property name="stopCause" type="java.lang.String" length="1" />
		<property name="visitEffect" type="java.lang.String" length="1" />
		<property name="visitDate" type="date" />
		<property name="stopDate" type="date" />
		<property name="visitDoctor" type="java.lang.String" length="20" />
		<property name="visitUnit" type="java.lang.String" length="20" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="inputDate" type="date" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.tr.schemas.MDC_TumourPatientVisit" table="MDC_TumourPatientVisit">
			<id name="visitId" type="java.lang.String">
            	<column name="visitId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="TPRCID" type="java.lang.String" length="16" />
        	<property name="annulControl" type="java.lang.String" length="1" />
        	<property name="annulDate" type="date"  />
        	<property name="annulCause" type="java.lang.String" length="1" />
        	<property name="otherCause" type="java.lang.String" length="200" />
        	<property name="haveTransfer" type="java.lang.String" length="1" />
        	<property name="transferPart" type="java.lang.String" length="200" />
        	<property name="isRelapse" type="java.lang.String" length="1" />
        	<property name="relapseNumber" type="java.lang.Integer" length="2" />
        	<property name="relapseDate1" type="date"  />
        	<property name="relapseDate2" type="date"  />
        	<property name="relapseDate3" type="date"  />
        	<property name="cureWay" type="java.lang.String" length="50" />
        	<property name="illnessCase" type="java.lang.String" length="1" />
        	<property name="weight" type="java.lang.Integer" length="2" />
        	<property name="guidanceContent" type="java.lang.String" length="2" />
        	<property name="CSV" type="java.lang.Integer" length="3" />
        	<property name="vistUnit" type="java.lang.String" length="20" />
        	<property name="visitDoctor" type="java.lang.String" length="20" />
        	<property name="visitDate" type="date"  />
        	<property name="remark" type="java.lang.String" length="500" />
        	<property name="isDeath" type="java.lang.String" length="1" />
        	<property name="deathDate" type="date"  />
        	<property name="deathPlace" type="java.lang.String" length="50" />
        	<property name="deathCause" type="java.lang.String" length="20" />
        	<property name="firstVisit" type="java.lang.String" length="1" />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
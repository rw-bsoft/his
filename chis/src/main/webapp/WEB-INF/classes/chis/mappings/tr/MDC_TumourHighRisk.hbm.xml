<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.tr.schemas.MDC_TumourHighRisk"
		table="MDC_TumourHighRisk">
		<id name="THRID" type="java.lang.String">
			<column name="THRID" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="32" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="highRiskType" type="java.lang.String" length="2" />
		<property name="turnHighRiskDate" type="date" />
		<property name="turnHighRiskDoctor" type="java.lang.String"
			length="10" />
		<property name="highRiskSource" type="java.lang.String"
			length="1" />
		<property name="nature" type="java.lang.String" length="1" />
		<property name="highRiskFactor" type="java.lang.String"
			length="300" />
		<property name="screeningPositive" type="java.lang.String" length="1" />
		<property name="screeningSickness" type="java.lang.String" length="1" />
			
		<property name="createStatus" type="java.lang.String" length="1" />
		<property name="createCardUser" type="java.lang.String" length="20" />
		<property name="createCardUnit" type="java.lang.String" length="20" />
		<property name="createCardDate" type="timestamp" />	
		<property name="timelyCreation" type="java.lang.String" length="1" />
			
		<property name="year" type="java.lang.Integer" length="4" />
		<property name="managerGroup" type="java.lang.String" length="1" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="highRiskMark" type="java.lang.String" length="1" />
		<property name="criterionMark" type="java.lang.String"
			length="1" />
		
		<property name="status" type="java.lang.String" length="1" />
		<property name="cancellationReason" type="java.lang.String"
			length="1" />
		<property name="cancellationUser" type="java.lang.String"
			length="20" />
		<property name="cancellationDate" type="date" />
		<property name="cancellationUnit" type="java.lang.String"
			length="20" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.ohr.schemas.MDC_OldPeopleVisit"
		table="MDC_OldPeopleVisit">
		<id name="visitId" type="java.lang.String">
			<column name="visitId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="visitType" type="java.lang.String" length="1" />
		<property name="visitEffect" type="java.lang.String" length="2" />
		<property name="noVisitReason" type="java.lang.String" 	length="100" />
		<property name="nowSymptoms" type="java.lang.String" length="20" />
		<property name="newSymptoms" type="java.lang.String" length="100" />
		<property name="weight" type="java.lang.Double" length="8" />
		<property name="sbp" type="java.lang.Integer" />
		<property name="dbp" type="java.lang.Integer" />
		<property name="smokeCount" type="java.lang.Integer" />
		<property name="drinkCount" type="java.lang.Integer" />
		<property name="drinkTypeCode" type="java.lang.String" 	length="64" />
		<property name="trainTimesWeek" type="java.lang.Integer" />
		<property name="trainMinute" type="java.lang.Integer" />
		<property name="food" type="java.lang.String" length="1" />
		<property name="psychologyChange" type="java.lang.String" length="1" />
		<property name="obeyDoctor" type="java.lang.String" length="1" />
		<property name="moveTreat" type="java.lang.String" length="1" />
		<property name="trainFreqCode" type="java.lang.String" length="2" />
		<property name="eateHabit" type="java.lang.String" length="64" />
		<property name="smokeFlag" type="java.lang.String" length="1" />
		<property name="drinkFlag" type="java.lang.String" length="2" />
		<property name="pastHistory" type="java.lang.String" length="100" />
		<property name="treat" type="java.lang.String" length="100" />
		<property name="medication" type="java.lang.String" length="100" />
		<property name="healthAssessment" type="java.lang.String" length="1" />
		<property name="healthException" type="java.lang.String" length="100" />
		<property name="healthGuidance" type="java.lang.String" length="10" />
		<property name="riskControll" type="java.lang.String" length="10" />
		<property name="targetWeight" type="java.lang.Double" length="8"/>
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputDate" type="timestamp" />
		<property name="visitUnit" type="java.lang.String" length="20" />
		<property name="visitDoctor" type="java.lang.String" length="20" />
		<property name="visitDate" type="date" />
		<property name="nextDate" type="date" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="CHDPrevent" type="java.lang.String" length="200" />
		<property name="OPPrevent" type="java.lang.String" length="200" />
		<property name="vaccine" type="java.lang.String" length="200" />
		<property name="heartState" type="java.lang.String" length="10" />
		<property name="visitItem" type="java.lang.String" length="200" />
		<property name="nextVisitTarget" type="java.lang.String" length="200" />
	</class>
</hibernate-mapping>
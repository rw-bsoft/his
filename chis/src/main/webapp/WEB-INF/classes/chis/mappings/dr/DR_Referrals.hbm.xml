<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class entity-name="DR_Referrals" table="dr_referrals" >
    	<id name="recordId" type="java.lang.Integer"		length="16">
        	<column name="RecordId"  />
        	<generator class="sequence">
				<param name="sequence">DR_REFERRALS_SEQ</param>
			</generator>
		</id> 
        <property name="mpiId" 			   type="java.lang.String" length="32"/>
        <property name="hospitalCode"      type="java.lang.String" length="30"/>
        <property name="departmentCode"    type="java.lang.String" length="20"/>
        <property name="DiseaseDescription" type="java.lang.String" length="500"/>
        <property name="announcements"       type="java.lang.String" length="500"/>
        <property name="turnReason"        type="java.lang.String" length="500"/>
        <property name="refuseReason"        type="java.lang.String" length="500"/>
        <property name="status"	           type="java.lang.String" length="30"/>
        <property name="submitorDoctor" 		   type="java.lang.String" length="30"/>
        <property name="submitAgency"      type="java.lang.String" length="40"/>
        <property name="submitTime"        type="timestamp" />
         <property name="reserveNo"         type="java.lang.String" length="16"/>
         <property name="businessType"         type="java.lang.String" length="20"/>
         <property name="applyInfo"         type="java.lang.String" length="200"/>
        <property name="operationTime"        type="date" />
        <property name="operator"         type="java.lang.String" length="50"/>
        <property name="isverify"	           type="java.lang.String" length="30"/>
 		<property name="isNew"	           type="java.lang.String" length="1"/>
        <property name="department"         type="java.lang.String" length="50"/>
        <property name="wardArea"         type="java.lang.String" length="50"/>
        <property name="bedNo"         type="java.lang.String" length="50"/>
        <property name="doctorName"         type="java.lang.String" length="50"/>
        <property name="doctorPhone"         type="java.lang.String" length="50"/>
        <property name="payType"	           type="java.lang.String" length="10"/>
        <property name="brxz"	           type="java.lang.Integer" length="18"/>
        <property name="emrNo"	           type="java.lang.String" length="20"/>
        <property name="hosNo"	           type="java.lang.String" length="20"/>
        <property name="ZZRQ"        type="date" />
        <property name="JYJCXM"	           type="java.lang.String" length="500"/>
        <property name="JYJCXMJG"	           type="java.lang.String" length="500"/>
        <property name="KFCSZD"	           type="java.lang.String" length="500"/>
        <property name="JZYS"	           type="java.lang.String" length="16"/>
        <property name="JYZRKS"	           type="java.lang.String" length="50"/>
		<property name="ststus"	           type="java.lang.String" length="50"/>
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class entity-name="chis.application.scm.schemas.SCM_ServiceContractPlanTask" table="SCM_ServiceContractPlanTask">
        <id name="taskId" type="java.lang.Long">
            <column name="taskId"/>
            <generator class="sequence">
			<param name="sequence">SEQ_SCM_CONTRACTPLANTASK</param>
			</generator>
        </id>
        <property name="empiId" type="java.lang.String" length="32"/>
        <property name="SCID" type="java.lang.Long" length="18"/>
        <property name="taskName" type="java.lang.String" length="200"/>
        <property name="taskCode" type="java.lang.String" length="100"/>
        <property name="moduleAppId" type="java.lang.String" length="200"/>
        <property name="status" type="java.lang.String" length="1"/>
        <property name="year" type="java.lang.Integer" length="5"/>
        <property name="planId" type="java.lang.String" length="16"/>
        <property name="visitId" type="java.lang.String" length="16"/>
        <property name="sort" type="java.lang.String" length="5"/>
        <property name="packageName" type="java.lang.String" length="30"/>
        <property name="SPID" type="java.lang.String" length="20"/>
         <property name="SPIID" type="java.lang.String" length="20"/>
    </class>
</hibernate-mapping>
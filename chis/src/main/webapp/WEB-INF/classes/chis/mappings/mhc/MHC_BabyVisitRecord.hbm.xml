<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mhc.schemas.MHC_BabyVisitRecord"
		table="MHC_BabyVisitRecord">
		<id name="visitId" type="java.lang.String">
			<column name="visitId" />
			<generator class="assigned" />
		</id>
		<property name="pregnantId" type="java.lang.String" length="30" />
		<property name="babyId" type="java.lang.String" length="16" />
		<property name="visitDate" type="date" />
		<property name="visitDoctor" type="java.lang.String"
			length="20" />
		<property name="visitUnit" type="java.lang.String" length="20" />
		<property name="temperature" type="big_decimal" length="4" />
		<property name="weight" type="big_decimal" length="5" />
		<property name="respiratoryFrequency" type="java.lang.Integer" />
		<property name="pulse" type="java.lang.Integer" />
		<property name="face" type="java.lang.String" length="1" />
		<property name="faceOther" type="java.lang.String" length="30" />
		<property name="bregmaTransverse" type="big_decimal" length="5" />
		<property name="bregmaLongitudinal" type="big_decimal"
			length="5" />
		<property name="bregmaStatus" type="java.lang.String"
			length="1" />
		<property name="otherStatus" type="java.lang.String"
			length="30" />
		<property name="eye" type="java.lang.String" length="2" />
		<property name="eyeAbnormal" type="java.lang.String"
			length="30" />
		<property name="ear" type="java.lang.String" length="1" />
		<property name="earAbnormal" type="java.lang.String"
			length="30" />
		<property name="nose" type="java.lang.String" length="1" />
		<property name="noseAbnormal" type="java.lang.String"
			length="30" />
		<property name="mouse" type="java.lang.String" length="2" />
		<property name="mouseAbnormal" type="java.lang.String"
			length="30" />
		<property name="thrush" type="java.lang.String" length="1" />
		<property name="heartlung" type="java.lang.String" length="1" />
		<property name="heartLungAbnormal" type="java.lang.String"
			length="30" />
		<property name="abdominal" type="java.lang.String" length="1" />
		<property name="abdominalabnormal" type="java.lang.String"
			length="30" />
		<property name="limbs" type="java.lang.String" length="1" />
		<property name="limbsAbnormal" type="java.lang.String"
			length="30" />
		<property name="neck" type="java.lang.String" length="1" />
		<property name="neck1" type="java.lang.String" length="30" />
		<property name="skin" type="java.lang.String" length="1" />
		<property name="skinAbnormal" type="java.lang.String"
			length="30" />
		<property name="anal" type="java.lang.String" length="1" />
		<property name="analAbnormal" type="java.lang.String"
			length="30" />
		<property name="genitalia" type="java.lang.String" length="1" />
		<property name="genitaliaAbnormal" type="java.lang.String"
			length="30" />
		<property name="spine" type="java.lang.String" length="1" />
		<property name="spineAbnormal" type="java.lang.String"
			length="30" />
		<property name="umbilical" type="java.lang.String" length="1" />
		<property name="umbilicalOther" type="java.lang.String"
			length="30" />
		<property name="stoolTimes" type="java.lang.Integer" />
		<property name="stoolDates" type="java.lang.Integer"  />
		<property name="stoolColor" type="java.lang.String" length="20" />
		<property name="stoolStatus" type="java.lang.String"
			length="20" />
		<property name="referral" type="java.lang.String" length="1" />
		<property name="referralReason" type="java.lang.String"
			length="50" />
		<property name="referralUnit" type="java.lang.String"
			length="50" />
		<property name="guide" type="java.lang.String" length="64" />
		<property name="nextVisitDate" type="date" />
		<property name="nextVisitAddress" type="java.lang.String"
			length="100" />
		<property name="createUnit" type="java.lang.String" length="20"/>
    	<property name="createUser" type="java.lang.String" length="20"/>
    	<property name="createDate" type="timestamp"/>
    	<property name="lastModifyUnit" type="java.lang.String" length="20"/>
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="feedWay" type="java.lang.String" length="2"/>
		<property name="eatNum" type="java.lang.Integer" length="20"/>
		<property name="eatCount" type="java.lang.Integer" length="5"/>
		<property name="vomit" type="java.lang.String" length="2"/>
		<property name="jaundice" type="java.lang.String" length="2"/>
		
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mobileApp.schemas.EHR_HealthRecordApp" table="EHR_HealthRecord">
		<id name="phrId" type="java.lang.String">
			<column name="phrId" />
			<generator class="assigned" />
		</id>
			<property name="manaDoctorId" type="java.lang.String"
			length="20" />
			<property name="masterFlag" type="java.lang.String" length="1" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="regionCode" type="java.lang.String" length="25" />
		<property name="regionCode_text" type="java.lang.String" length="200" />
		<property name="isAgrRegister" type="java.lang.String"
			length="1" />
		<!-- 
			<property name="homeAddress" type="java.lang.String" length="21" />
		-->
		<property name="signFlag" type="java.lang.String" length="1" />	
		<property name="incomeSource" type="java.lang.String" length="1" />
		<property name="familyDoctorId" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
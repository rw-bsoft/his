<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="chis.application.mhc.schemas.MHC_PostnatalVisitInfo"
    table="MHC_PostnatalVisitInfo">
    <id name="visitId" type="java.lang.String">
      <column name="visitId" />
      <generator class="assigned" />
    </id>
    <property name="empiId" type="java.lang.String" length="32" />
    <property name="pregnantId" type="java.lang.String" length="16" />
    <property name="visitDate" type="date" />
    <property name="healthState" length="64"
      type="java.lang.String" />
    <property name="psychologyState" length="64"
      type="java.lang.String" />
    <property name="birthDay" type="date" />
    <property name="postnatalDays" length="2"
      type="java.lang.Integer" />
    <property name="temperature" type="big_decimal" />
    <property name="pulse" type="java.lang.Integer" />
    <property name="constriction" type="java.lang.Integer" />
    <property name="diastolic" type="java.lang.Integer" />
    <property name="breast" type="java.lang.String" />
    <property name="breastText" type="java.lang.String"
      length="200" />
    <property name="breastBump" type="java.lang.String" />
    <property name="nipple" type="java.lang.String" />
    <property name="latex" type="java.lang.String" />
    <property name="palace" type="java.lang.Integer" />
    <property name="wound" type="java.lang.String" />
    <property name="woundText" type="java.lang.String"
      length="200" />
    <property name="healing" type="java.lang.String" />
    <property name="infectant" type="java.lang.String" />
    <property name="lochia" type="java.lang.String" />
    <property name="lochiaText" type="java.lang.String"
      length="200" />
    <property name="lochiaColor" type="java.lang.String" />
    <property name="lochiaStench" type="java.lang.String" />
    <property name="lochiaMeasure" type="java.lang.String" />
    <property name="uterus" type="java.lang.String" />
    <property name="uterusText" type="java.lang.String"
      length="200" />
    <property name="excrement" type="java.lang.String" />
    <property name="piss" type="java.lang.String" />
    <property name="other" length="200"
      type="java.lang.String" />
    <property name="classification" type="java.lang.String"
      length="1" />
    <property name="classificationText" length="200"
      type="java.lang.String" />
    <property name="specialCases" length="200"
      type="java.lang.String" />
    <property name="suggestion" length="45"
      type="java.lang.String" />
    <property name="otherSuggestion" type="java.lang.String" length="200" />
    <property name="checkDoctor" type="java.lang.String"
      length="20" />
    <property name="checkUnit" type="java.lang.String" length="20" />
    <property name="nextVisitDate" type="date" />
    <property name="checkManaUnit" type="java.lang.String" length="20"/>
    <property name="referral" type="java.lang.String" length="1" />
    <property name="reason" type="java.lang.String" length="200" />
    <property name="doccol" type="java.lang.String" length="50" />
    <property name="referralNum" type="java.lang.String"
      length="16" />
    <property name="createUnit" type="java.lang.String" length="20"/>
    <property name="createUser" type="java.lang.String" length="20"/>
    <property name="createDate" type="timestamp"/>
    <property name="lastModifyUnit" type="java.lang.String" length="20"/>
    <property name="lastModifyDate" type="timestamp" />
    <property name="lastModifyUser" type="java.lang.String"
      length="20" />
  </class>
</hibernate-mapping>
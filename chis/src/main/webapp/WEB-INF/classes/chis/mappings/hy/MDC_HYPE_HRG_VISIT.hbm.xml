<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.hy.schemas.MDC_HYPE_HRG_VISIT" table="MDC_HYPE_HRG_VISIT">
			<id name="visitId" type="java.lang.String">
            	<column name="visitId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="visitDate" type="date"  />
        	<property name="visitDoctor" type="java.lang.String" length="20" />
        	<property name="visitWay" type="java.lang.String" length="1" />
        	<property name="constriction" type="java.lang.Double" length="3" />
        	<property name="diastolic" type="java.lang.Double" length="3" />
        	<property name="waistline" type="java.lang.Double" length="3" />
        	<property name="weight" type="java.lang.Double" length="3" />
        	<property name="targetWeight" type="java.lang.Double" length="3" />
        	<property name="BMI" type="java.lang.Double" length="8" />
        	<property name="BFDI" type="java.lang.String" length="1" />
        	<property name="riskiness" type="java.lang.String" length="100" />
        	<property name="healthGuidance" type="java.lang.String" length="4000" />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="22" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="22" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
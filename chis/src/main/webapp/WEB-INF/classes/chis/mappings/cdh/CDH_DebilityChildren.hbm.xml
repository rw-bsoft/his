<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.cdh.schemas.CDH_DebilityChildren"
		table="CDH_DebilityChildren">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="phoneNo" type="java.lang.String" length="16" />
		<property name="feedWay" type="java.lang.String" length="1" />
		<property name="debilityReason" type="java.lang.String"
			length="2" />
		<property name="vestingCode" type="java.lang.String" length="1" />
		<property name="closedDate" type="date" />
		<property name="closedDoctor" type="java.lang.String"
			length="20" />
		<property name="closedUnit" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp" />
		<property name="createUser" type="java.lang.String"
			length="20" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="planTypeCode" type="java.lang.String"
			length="2" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="debilityOtherReason" type="java.lang.String"
			length="64" />
		<property name="deseaseReason" type="java.lang.String"
			length="64" />
		<property name="deseaseOtherReason" type="java.lang.String"
			length="64" />
		<property name="otherVesting" type="java.lang.String"
			length="64" />

		<property name="closeFlag" type="java.lang.String" />
		
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="cancellationUser" type="java.lang.String" length="20" />
		<property name="cancellationDate" type="date" />
		<property name="cancellationUnit" type="java.lang.String" length="20" />
		<property name="cancellationReason" type="java.lang.String" length="1" />
		<property name="deadReason" type="java.lang.String" length="100" />
	</class>
</hibernate-mapping>
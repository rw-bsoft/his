<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.cdh.schemas.CDH_BirthCertificate" table="CDH_BirthCertificate">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="phrId"        type="java.lang.String" length="30" />
		
		<property name="certificateIDold"     type="java.lang.String" length="10" />
		<!--<property name="certificateId"        type="java.lang.String" length="16" />
			<property name="babyName"             type="java.lang.String" length="30" />
			<property name="babySex"              type="java.lang.String" length="1" />
			<property name="babyBirthday"         type="date"/>
			<property name="birthAddress"         type="java.lang.String" length="120" />  -->
		<property name="birthAddressType"     type="java.lang.String" length="1" />
		<property name="healthStatus"         type="java.lang.String" length="1" />
		<property name="gestation"            type="int"/>
		<property name="height"               type="java.lang.Double" />
		<property name="weight"               type="java.lang.Double" />
		<property name="motherName"           type="java.lang.String" length="30" />
		<property name="motherNationality"    type="java.lang.String" length="3" />
		<property name="motherNation"         type="java.lang.String" length="2" />
		<property name="motherCardNo"         type="java.lang.String" length="18" />
		<property name="fatherName"           type="java.lang.String" length="30" />
		<property name="fatherNationality"    type="java.lang.String" length="3" />
		<property name="fatherNation"         type="java.lang.String" length="2" />
		<property name="fatherCardNo"         type="java.lang.String" length="18" />
		<property name="deliveryDoctor"       type="java.lang.String" length="30" />
		<property name="deliveryUnit"         type="java.lang.String" length="70"/>
		<property name="issueDate"            type="date"/>
		<property name="issueUnit"            type="java.lang.String" length="70" />
		<property name="lastModifyUser"       type="string"           length="20"/>
		<property name="lastModifyUnit"       type="java.lang.String" length="20" />
		<property name="lastModifyDate"       type="timestamp"                        />
		<property name="createUser"           type="string"           length="20"/>
		<property name="createUnit"           type="string"           length="20"/>
		<property name="createDate"           type="timestamp"                        />
		<property name="empiId"               type="string"           length="32"/>
	</class>
</hibernate-mapping>
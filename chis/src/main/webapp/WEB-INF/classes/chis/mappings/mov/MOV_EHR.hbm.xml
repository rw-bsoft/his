<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.mov.schemas.MOV_EHR" table="MOV_EHR">
		<id name="archiveMoveId" type="java.lang.String">
			<column name="archiveMoveId" />
			<generator class="assigned" />
		</id>
		<property name="archiveType" type="java.lang.String" length="1" />
		<property name="moveType" type="java.lang.String" length="1" />
		<property name="movesub" type="java.lang.String" length="1" />
		<property name="archiveId" type="java.lang.String" length="30" />
		<property name="personName" type="java.lang.String" length="20"/>
		<property name="sourceArea" type="java.lang.String" length="25" />
		<property name="sourceArea_text" type="java.lang.String" length="200" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="sourceUnit" type="java.lang.String" length="20" />
		<property name="sourceDoctor" type="java.lang.String" length="20" />
		<property name="targetArea" type="java.lang.String" length="25" />
		<property name="targetArea_text" type="java.lang.String" length="200" />
		<property name="targetUnit" type="java.lang.String" length="20"  />
		<property name="targetDoctor" type="java.lang.String" length="20" />
		<property name="applyUser" type="java.lang.String" length="20" />
		<property name="applyUnit" type="java.lang.String" length="20" />
		<property name="applyDate" type="timestamp" />
		<property name="applyReason" type="java.lang.String" length="500" />
		<property name="affirmUser" type="java.lang.String" length="20" />
		<property name="affirmType" type="java.lang.String" length="1" />
		<property name="affirmUnit" type="java.lang.String" length="20" />
		<property name="affirmDate" type="timestamp" />
		<property name="affirmView" type="java.lang.String" length="500" />
		<property name="lastModifyUnit" type="java.lang.String" length="20"/>
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.pd.schemas.CDH_WHOBMI" table="CDH_WHOBMI">
			<id name="recordId" type="java.lang.String">
            	<column name="recordId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="age" type="java.lang.Integer" />
        	<property name="ageNote" type="java.lang.String" length="20" />
        	<property name="sexCode" type="java.lang.String" length="1" />
        	<property name="BMIL" type="double" length="8" />
        	<property name="BMIM" type="double" length="8" />
        	<property name="BMIS" type="double" length="8" />
        	<property name="BMISD3neg" type="double" length="5" />
        	<property name="BMISD2neg" type="double" length="5" />
        	<property name="BMISD1neg" type="double" length="5" />
        	<property name="BMISD0" type="double" length="5" />
        	<property name="BMISD1" type="double" length="5" />
        	<property name="BMISD2" type="double" length="5" />
        	<property name="BMISD3" type="double" length="5" />
</class>
</hibernate-mapping>
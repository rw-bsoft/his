<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.tr.schemas.MDC_TumourScreeningCheckResult" table="MDC_TumourScreeningCheckResult">
			<id name="recordId" type="java.lang.String">
            	<column name="recordId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="screeningId" type="java.lang.String" length="16" />
        	<property name="planId" type="java.lang.String" length="16" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="itemId" type="java.lang.String" length="16" />
        	<property name="checkItem" type="java.lang.String" length="100" />
        	<property name="checkResult" type="java.lang.String" length="30" />
        	<property name="checkDate" type="date"  />
        	<property name="checkHospital" type="java.lang.String" length="30" />
        	<property name="checkDoctor" type="java.lang.String" length="20" />
        	<property name="criterionType" type="java.lang.String" length="1" />
        	<property name="remark" type="java.lang.String" length="300" />
        	<property name="highRiskType" type="java.lang.String" length="2" />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
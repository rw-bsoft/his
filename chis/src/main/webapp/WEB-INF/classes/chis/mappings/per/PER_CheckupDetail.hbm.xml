<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.per.schemas.PER_CheckupDetail" table="PER_CheckupDetail">
		<id name="id" type="java.lang.String" length="16">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="checkupNo" type="java.lang.String" length="30" />
		<property name="recordNo" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="checkupProjectId" type="java.lang.String"
			length="16" />
		<property name="checkupProjectName" type="java.lang.String"
			length="60" />
		<property name="checkupOutcome" type="java.lang.String"
			length="1" />
		<property name="checkupUnit" type="java.lang.String"
			length="20" />
		<property name="referenceUpper" type="java.lang.Double"
			length="8" />
		<property name="referenceLower" type="java.lang.Double"
			length="8" />
		<property name="projectOffice" type="java.lang.String"
			length="50" />
		<property name="projectOfficeCode" type="java.lang.String"
			length="16" />
		<property name="projectType" type="java.lang.String" length="2" />
		<property name="projectClass" type="java.lang.String"
			length="2" />
		<property name="ifException" type="java.lang.String" />
		<property name="memo" type="java.lang.String" />
		<property name="createUnit" type="java.lang.String" length="20"/>
    	<property name="createUser" type="java.lang.String" length="20"/>
    	<property name="createDate" type="timestamp"/>
    	<property name="lastModifyUnit" type="java.lang.String" length="20"/>
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
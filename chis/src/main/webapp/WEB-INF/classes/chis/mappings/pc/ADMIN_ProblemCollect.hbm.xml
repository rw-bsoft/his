<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.pc.schemas.ADMIN_ProblemCollect" table="ADMIN_ProblemCollect">
		<id name="problemId" type="java.lang.String">
			<column name="problemId"/>
			<generator class="assigned" />
		</id>
		<property name="description" type="java.lang.String" length="2000" />
		<property name="contactPhone" type="java.lang.String" length="20" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createTime" type="timestamp"  />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="status" type="java.lang.String" length="2" />
		<property name="treatTime" type="date" />
		<property name="treatDesc" type="java.lang.String" length="500" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
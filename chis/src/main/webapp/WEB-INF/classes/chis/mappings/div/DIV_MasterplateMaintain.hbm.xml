<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="DIV_MasterplateMaintain" table="DIV_MasterplateMaintain">
			<id name="masterplateId" type="java.lang.String">
            	<column name="masterplateId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="masterplateName" type="java.lang.String" length="100" />
        	<property name="masterplateType" type="java.lang.String"   length="2"/>
        	<property name="dateType" type="java.lang.String"  />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="date"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="date"  />
</class>
</hibernate-mapping>
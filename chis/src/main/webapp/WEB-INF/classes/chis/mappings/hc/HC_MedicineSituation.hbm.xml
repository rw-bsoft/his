<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="chis.application.hc.schemas.HC_MedicineSituation" table="HC_MedicineSituation">
		<id name="situationId" length="16" type="java.lang.String">
			<column name="situationId"/>
			<generator class="assigned"/>
		</id>
		<property name="healthCheck" length="16" type="java.lang.String"/>
		<property name="medicine" length="50" type="java.lang.String"/>
		<property name="eachDose" length="50" type="java.lang.String"/>
		<property name="use" type="java.lang.String"/>
		<property name="descr"  type="java.lang.String"/>
		<property name="useDate"  type="java.lang.String"/>
		<property name="medicineYield"  type="java.lang.String"/>
		<property name="createUnit" length="20" type="java.lang.String"/>
		<property name="createDate" type="timestamp"/>
		<property name="createUser" length="20" type="java.lang.String"/>
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>

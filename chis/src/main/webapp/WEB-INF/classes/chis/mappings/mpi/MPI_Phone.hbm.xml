<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.mpi.schemas.MPI_Phone" table="MPI_Phone">
		<id name="phoneId" type="java.lang.String" length="16">
			<column name="phoneId" />
			<generator class="assigned" />
		</id>
	<property name="empiId"             	type="java.lang.String" length="32"/>
    <property name="phoneTypeCode"      	type="java.lang.String" length="2"/>
    <property name="phoneNo"            	type="java.lang.String" length="20"/>
</class>
</hibernate-mapping>
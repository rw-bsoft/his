<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="chis.application.mhc.schemas.MHC_FirstVisitRecord"
    table="MHC_FirstVisitRecord">
    <id name="pregnantId" type="java.lang.String">
      <column name="pregnantId" />
      <generator class="assigned" />
    </id>
    <property name="empiId" type="java.lang.String" length="32" />
    <property name="diagnosisMethod" type="java.lang.String"
      length="1" />
    <property name="diagnosisDate" type="date" />
    <property name="morningSickness" type="java.lang.String"
      length="1" />
    <property name="morningSicknessWeek" type="java.lang.Integer" />
    <property name="selfFeelSymptom" type="java.lang.String"
      length="2" />
    <property name="symptomDesc" type="java.lang.String"
      length="100" />
    <property name="BMI" type="java.lang.String" length="10" />
    <property name="weight" type="java.lang.Double" />
    <property name="dbp" type="java.lang.Integer" />
    <property name="sbp" type="java.lang.Integer" />
    <property name="quickeningWeek" type="java.lang.Integer" />
    <property name="headache" type="java.lang.String" length="1" />
    <property name="headacheWeek" type="java.lang.String"
      length="20" />
    <property name="heightFundusUterus" type="java.lang.Double" />
    <property name="abdomenCircumFerence" type="java.lang.Double" />
    <property name="edema" type="java.lang.String" length="1" />
    <property name="edemaWeek" type="java.lang.Integer" />
    <property name="bleeding" type="java.lang.String" length="1" />
    <property name="bleedingWeek" type="java.lang.Integer" />
    <property name="fever" type="java.lang.String" length="1" />
    <property name="feverWeek" type="java.lang.Integer" />
    <property name="otherSympton" type="java.lang.String"
      length="100" />
    <property name="gestationalWeeks" type="java.lang.String"
      length="20" />
    <property name="diagnoseDate" type="date" />
    <property name="diagnoseResult" type="java.lang.String" length="1" />
    <property name="screeningDate" type="date" />
    <property name="screening" type="java.lang.String" length="1" />
    <property name="generalComment" type="java.lang.String"
      length="100" />
    <property name="commentText" type="java.lang.String"
      length="200" />
    <property name="highRiskReason" type="java.lang.String"
      length="64" />
    <property name="highRiskLevel" type="java.lang.String"
      length="64" />
    <property name="highRiskScore" type="java.lang.Integer" />
    <property name="suggestion" length="45" type="java.lang.String" />
    <property name="otherSuggestion" type="java.lang.String" length="200" />
    <property name="referral" type="java.lang.String" length="1" />
    <property name="reason" type="java.lang.String" length="200" />
    <property name="doccol" type="java.lang.String" length="50" />
    <property name="visitPrecontractTime" type="date" />
    <property name="visitDoctorCode" type="java.lang.String"
      length="20" />
    <property name="visitUnitCode" type="java.lang.String"
      length="20" />
    <property name="category" type="java.lang.String" length="2" />
    <property name="createUnit" type="java.lang.String" length="20"/>
    <property name="createUser" type="java.lang.String" length="20"/>
    <property name="createDate" type="timestamp"/>
    <property name="lastModifyUnit" type="java.lang.String" length="20"/>
    <property name="lastModifyDate" type="timestamp" />
    <property name="lastModifyUser" type="java.lang.String"
      length="20" />
  </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.psy.schemas.PSY_RecordPaper" table="PSY_RecordPaper">
			<id name="recordPaperId" type="java.lang.String">
            	<column name="recordPaperId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="content" type="java.lang.String" length="1000" />
        	<property name="phrId" type="java.lang.String" length="30" />
        	<property name="createUnit" type="java.lang.String" length="20" />
			<property name="createUser" type="java.lang.String" length="20" />
			<property name="createDate" type="timestamp" />
</class>
</hibernate-mapping>
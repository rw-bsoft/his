<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hr.schemas.EHR_PersonProblem" table="EHR_PersonProblem">
		<id name="sickRecordId" type="java.lang.String">
			<column name="sickRecordId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="cycleId" type="string" length="2" />
		<property name="problemName" type="string" length="80" />
		<property name="description" type="string" length="500" />
		<property name="result" type="string" length="500" />
		<property name="occurDate" type="date" />
		<property name="solveDate" type="date" />
		<property name="recordUser" type="string" length="20" />
		<property name="recordUnit" type="string" length="16" />
		<property name="recordDate" type="timestamp" />
		<property name="pastHistoryId" type="java.lang.String" length="16" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
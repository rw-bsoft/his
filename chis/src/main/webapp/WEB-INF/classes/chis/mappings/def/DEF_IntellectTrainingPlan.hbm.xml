<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.def.schemas.DEF_IntellectTrainingPlan" table="DEF_IntellectTrainingPlan">
			<id name="id" type="java.lang.String">
            	<column name="id"/>
            	<generator class="assigned" />
        	</id>
        	<property name="defId" type="java.lang.String" length="16" />
        	<property name="healingTarget" type="java.lang.String" length="100" />
        	<property name="healingTraining" type="java.lang.String" length="100" />
        	<property name="healingMaterial" type="java.lang.String" length="100" />
        	<property name="healingPlace" type="java.lang.String" length="20" />
        	<property name="healingTrainer" type="java.lang.String" length="20" />
        	<property name="healingMan" type="java.lang.String" length="20" />
        	<property name="healingDate" type="date"  />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="timestamp"  />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
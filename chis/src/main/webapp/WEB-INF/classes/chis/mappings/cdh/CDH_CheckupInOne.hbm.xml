		<property name="checkDoctor" type="java.lang.String"       length="20" />
		<property name="checkUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUser" type="java.lang.String"       length="20" />  
		<property name="kylgbz" type="java.lang.String"       length="1" />
		<property name="kyglbtz" type="java.lang.String" length="1" />
		<property name="tlxwgc" type="java.lang.String" length="1" />
		<property name="hwhd" type="java.lang.Double"/>
		<property name="fywss" type="java.lang.Double"/>
		<property name="hbqk" type="java.lang.String" length="1" />

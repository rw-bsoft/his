		<property name="ear" type="java.lang.String" length="1" />
		<property name="mouse" type="java.lang.String" length="2" />
		<property name="decayedTooth" type="int" />
		<property name="heartLung" type="java.lang.String" length="1" />
		<property name="abdomen" type="java.lang.String" length="1" />
		<property name="navel" type="java.lang.String" length="1" />
		<property name="navelState" type="java.lang.String" length="1" />
		<property name="extremities" type="java.lang.String" length="1" />
		<property name="genitals" type="java.lang.String" length="2" />
		<property name="hgb"  type="java.lang.Double"/>
		<property name="hearing" type="java.lang.String" length="1" />
		<property name="development" type="java.lang.String" length="1" />

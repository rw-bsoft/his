<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.her.schemas.HER_FileUpload" table="HER_FileUpload">
		<id name="fileId" type="java.lang.String">
			<column name="fileId"/>
			<generator class="assigned" />
		</id>
		<property name="setId" type="java.lang.String" length="16" />
		<property name="exeId" type="java.lang.String" length="16" />
		<property name="recordId" type="java.lang.String" length="16" />
		<property name="fileName" type="java.lang.String" length="200"/>
		<property name="fileType" type="java.lang.String" length="2"/>
		<property name="fileObjectType" type="java.lang.String" length="2"/>
		<property name="fileSite" type="java.lang.String" length="2"/>
		
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp"/>
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.psy.schemas.PSY_PsychosisRecord"
		table="PSY_PsychosisRecord">
		<id name="phrId" type="java.lang.String">
			<column name="phrId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="guardianName" type="java.lang.String"
			length="20" />
		<property name="guardianRelation" type="java.lang.String"
			length="30" />
		<property name="guardianAddress" type="java.lang.String"
			length="25" />
		<property name="guardianAddress_text" type="java.lang.String"
			length="200" />
		<property name="guardianPhone" type="java.lang.String"
			length="20" />
		<property name="VNCLinkMan" type="java.lang.String" length="20"/>
		<property name="VNCLinkPhone" type="java.lang.String" length="20"/>
		<property name="informedConsent" type="java.lang.String" length="1"/>
		<property name="icSign" type="java.lang.String" length="30"/>
		<property name="icDate" type="date" />
		<property name="economicConditions" type="java.lang.String" length="1"/>
		<property name="diseasedTime" type="date" />
		<property name="firstCureTime" type="date" />
		<property name="pastSymptom" type="java.lang.String"
			length="50" />
		<property name="pastSymptomText" type="java.lang.String"
			length="50" />
		<property name="pastClinic" type="java.lang.String" length="2" />
		<property name="pastHospitalization" type="java.lang.String" />
		<property name="pastHospitalizationText"
			type="java.lang.Integer" />
		<property name="recentDiagnosis" type="java.lang.String"
			length="20" />
		<property name="recentDiagnoseHospital" type="java.lang.String"
			length="20" />
		<property name="recentDiagnoseTime" type="date" />
		<property name="recentTreatResult" type="java.lang.String"
			length="2" />
		<property name="familySocialImpact" type="java.lang.String" length="20"/>
		<property name="lightAffray" type="java.lang.Integer" />
		<property name="causeTrouble" type="java.lang.Integer" />
		<property name="causeAccident" type="java.lang.Integer" />
		<property name="selfHurt" type="java.lang.Integer" />
		<property name="suicide" type="java.lang.Integer" />
		<property name="specialistOpinion" type="java.lang.String" length="2000"/>
		<property name="manaDoctorId" type="java.lang.String"
			length="20" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="closeDoor" type="java.lang.String" length="1" />

		<property name="status" type="java.lang.String" length="1" />
		<property name="cancellationCheckUnit" type="java.lang.String"
			length="16" />
		<property name="cancellationCheckUser" type="java.lang.String"
			length="20" />
		<property name="cancellationCheckDate" type="date" />
		
		<property name="fillTableDate" type="date" />
		<property name="doctorSign" type="java.lang.String" length="20" />
		<property name="doctorUnit" type="java.lang.String" length="20" />
		
		<property name="createDate" type="timestamp" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="cancellationUser" type="java.lang.String" length="20" />
		<property name="cancellationDate" type="date" />
		<property name="cancellationUnit" type="java.lang.String" length="20" />
		<property name="cancellationReason" type="java.lang.String" length="1" />
		<property name="deadReason" type="java.lang.String" length="100" />

	</class>
</hibernate-mapping>
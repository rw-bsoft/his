<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.cdh.schemas.CDH_Accident" table="CDH_Accident">
		<id name="accidentId" type="java.lang.String">
			<column name="accidentId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<!-- 
			<property name="manaDoctorId" type="java.lang.String" length="8" />
		-->
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="oneYearLive" type="java.lang.String" length="1" />
		<property name="accidentDate" type="date" />
		<property name="accidentPlace" type="java.lang.String"
			length="1" />
		<property name="otherPlace" type="java.lang.String" length="20" />
		<property name="inOutRoom" type="java.lang.String" length="1" />
		<property name="presentGuardian" type="java.lang.String"
			length="1" />
		<property name="otherDescribe" type="java.lang.String"
			length="20" />
		<property name="trainFlag" type="java.lang.String" length="1" />
		<property name="accidentDiagnosis" type="java.lang.String"
			length="100" />
		<property name="accidentType" type="java.lang.String"
			length="2" />
		<property name="firstManageArea" type="java.lang.String"
			length="1" />
		<property name="otherManageArea" type="java.lang.String"
			length="20" />
		<property name="firstHandler" type="java.lang.String"
			length="1" />
		<property name="otherHandler" type="java.lang.String"
			length="20" />
		<property name="treatHospital" type="java.lang.String"
			length="50" />
		<property name="hospitalLevel" type="java.lang.String"
			length="1" />
		<property name="levelResult" type="java.lang.String" length="1" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputDate" type="timestamp" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
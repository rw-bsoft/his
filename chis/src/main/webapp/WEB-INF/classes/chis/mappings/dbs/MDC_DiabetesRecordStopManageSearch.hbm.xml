<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.dbs.schemas.MDC_DiabetesRecordStopManageSearch"
		table="MDC_DiabetesRecord">
		<id name="phrId" type="java.lang.Integer">
			<column name="phrId" />
			<generator class="identity" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="height" type="java.lang.Double" length="8" />
		<property name="manaUnitId" type="java.lang.String" length="16" />
		<property name="manaDoctorID" type="java.lang.String"
			length="20" />
		<property name="diabetesType" type="java.lang.String"
			length="1" />
		<property name="caseSource" type="java.lang.String" length="1" />
		<property name="diagnosisDate" type="date" />
		<property name="diagnosisUnit" type="java.lang.String"
			length="16" />
		<property name="fbs" type="java.lang.Double" length="8" />
		<property name="pbs" type="java.lang.Double" length="8" />
		<property name="complication" type="java.lang.String"
			length="16" />
		<property name="familyHistroy" type="java.lang.String"
			length="1" />
		<property name="diabetesGroup" type="java.lang.String"
			length="2" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp" />
		<property name="createUnit" type="java.lang.String" length="16" />
		
		<property name="cancellationDate" type="date" />
		<property name="cancellationReson" type="java.lang.String"
			length="1" />
		<property name="cancellationCheckUser" type="java.lang.String"
			length="20" />
		<property name="cancellationCheckDate" type="date" />
		<property name="cancellationCheckUnit" type="java.lang.String"
			length="20" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="yearEstimate" type="date" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>
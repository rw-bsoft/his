<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="chis.application.hc.schemas.HC_HealthAssessment" table="HC_HealthAssessment">
		<id name="assessmentId" length="16" type="java.lang.String">
			<column name="assessmentId"/>
			<generator class="assigned"/>
		</id>
		<property name="healthCheck" length="16" type="java.lang.String"/>
		<property name="recognize"  type="java.lang.String"/>
		<property name="abnormality"  type="java.lang.String"/>
		<property name="abnormality1"  type="java.lang.String"/>
		<property name="abnormality2"  type="java.lang.String"/>
		<property name="abnormality3"  type="java.lang.String"/>
		<property name="abnormality4"  type="java.lang.String"/>
		<property name="mana"  type="java.lang.String"/>
		<property name="riskfactorsControl"  type="java.lang.String"/>
		<property name="targetWeight"  length="8" precision="2" type="java.lang.Double"/>
		<property name="vaccine"  type="java.lang.String"/>
		<property name="pjOther"  type="java.lang.String"/>
		<property name="createUnit" length="20" type="java.lang.String"/>
		<property name="createDate" type="timestamp"/>
		<property name="createUser" length="20" type="java.lang.String"/>
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.dbs.schemas.MDC_DiabetesSimilarityCheck" table="MDC_DiabetesSimilarity">
		<id name="similarityId" type="java.lang.String">
			<column name="similarityId"/>
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="sourceId" type="java.lang.String" length="32" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="riskiness" type="java.lang.String" length="64" />
		<property name="fbs" type="java.lang.Double" length="8" />
		<property name="pbs" type="java.lang.Double" length="8" />
		<property name="weight" type="java.lang.Double" length="8" />
		<property name="height" type="java.lang.Double" length="8" />
		<property name="bmi" type="java.lang.Double" length="8" />
		<property name="clinicalSymptom" type="java.lang.String" length="1" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="inputDate" type="timestamp"  />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="diagnosisType" type="java.lang.String" length="1" />
		<property name="diagnosisUnit" type="java.lang.String" length="20" />
		<property name="diagnosisDate" type="date"  />
		<property name="diagnosisDoctor" type="java.lang.String" length="20" />
		<property name="registerDate"  type="date" />
		<property name="registerUser" type="java.lang.String" length="20"/>
		<property name="registerUnit" type="java.lang.String" length="20"/>
		<property name="fbs1"  type="java.lang.Double" length="6"/>
		<property name="pbs1"  type="java.lang.Double" length="6"/>
		<property name="result1" type="java.lang.String" length="1"/>
		<property name="checkUser1" type="java.lang.String" length="20"/>
		<property name="checkDate1" type="date"/>
		<property name="clinicSymptom1" type="java.lang.String" length="1"/>
		<property name="fbs2" type="java.lang.Double" length="6"/>
		<property name="pbs2" type="java.lang.Double" length="6"/>
		<property name="result2" type="java.lang.String" length="1"/>
		<property name="checkUser2" type="java.lang.String" length="20"/>
		<property name="checkDate2" type="date"/>
		<property name="clinicSymptom2" type="java.lang.String" length="1"/>
	</class>
</hibernate-mapping>
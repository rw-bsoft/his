<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hy.schemas.MDC_HypertensionClinicRecord"
		table="MDC_HypertensionClinicRecord">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="height" type="java.lang.Double" />
		<property name="weight" type="java.lang.Double" />
		<property name="constriction" type="java.lang.Integer" />
		<property name="diastolic" type="java.lang.Integer" />
		<property name="clinicUser" type="java.lang.String" length="20" />
		<property name="clinicDate" type="date"/>
		<property name="clinicUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
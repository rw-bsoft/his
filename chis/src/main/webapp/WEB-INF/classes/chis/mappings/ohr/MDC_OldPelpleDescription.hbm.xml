<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.ohr.schemas.MDC_OldPelpleDescription"
		table="MDC_OldPelpleDescription">
		<id name="visitId" type="java.lang.String">
			<column name="visitId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="description" type="java.lang.String" length="2000" />
		<property name="inputDate" type="timestamp" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
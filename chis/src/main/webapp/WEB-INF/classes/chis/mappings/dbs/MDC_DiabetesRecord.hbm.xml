<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.dbs.schemas.MDC_DiabetesRecord"
		table="MDC_DiabetesRecord">
		<id name="phrId" type="java.lang.String">
			<column name="phrId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="height" type="java.lang.Double" length="8" />
		<property name="weight" type="java.lang.Double" length="8" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="manaDoctorId" type="java.lang.String"
			length="20" />
		<property name="diabetesType" type="java.lang.String"
			length="1" />
		<property name="caseSource" type="java.lang.String" length="1" />
		<property name="diagnosisDate" type="date" />
		<property name="diagnosisUnit" type="java.lang.String"
			length="100" />
		<property name="fbs" type="java.lang.Double" length="8" />
		<property name="pbs" type="java.lang.Double" length="8" />
		<property name="unit" type="java.lang.String" length="1" />
		<property name="familyHistroy" type="java.lang.String"
			length="1" />
		<property name="riskFactors" type="java.lang.String" length="50" />
		<property name="diabetesGroup" type="java.lang.String"
			length="2" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createDate" type="date" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="cancellationCheckUser" type="java.lang.String"
			length="20" />
		<property name="cancellationCheckDate" type="date" />
		<property name="cancellationCheckUnit" type="java.lang.String"
			length="20" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="yearEstimate" type="date" />
		<property name="planTypeCode" type="java.lang.String" length="2" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="cancellationUser" type="java.lang.String" length="20" />
		<property name="cancellationDate" type="date" />
		<property name="cancellationUnit" type="java.lang.String" length="20" />
		<property name="cancellationReason" type="java.lang.String" length="1" />
		<property name="deadReason" type="java.lang.String" length="100" />
		<property name="endCheck" type="java.lang.String" length="1" />
		<property name="visitEffect" type="java.lang.String" length="1" />
		<property name="noVisitReason" type="java.lang.String" length="1" />
		<property name="visitDate" type="date"/>
		<property name="deadDate" type="date"/>
		<property name="lastFixGroupDate" type="date"/>
		<property name="nextVisitDate" type="date"/>
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputDate" type="timestamp" />
		<property name="inputUnit" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
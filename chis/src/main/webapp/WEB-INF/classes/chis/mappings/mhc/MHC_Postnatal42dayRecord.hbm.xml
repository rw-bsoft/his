<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class entity-name="chis.application.mhc.schemas.MHC_Postnatal42dayRecord"
    table="MHC_Postnatal42dayRecord">
    <id name="recordId" type="java.lang.String">
      <column name="recordId" />
      <generator class="assigned" />
    </id>
    <property name="empiId" type="java.lang.String" length="32" />
    <property name="checkDate" type="date" />
    <property name="postnatalDays" length="2"
      type="java.lang.Integer" />
    <property name="pregnantId" length="16" type="java.lang.String" />
    <property name="healthState" length="64"
      type="java.lang.String" />
    <property name="psychologyState" length="64"
      type="java.lang.String" />
    <property name="constriction" type="java.lang.Integer" />
    <property name="diastolic" type="java.lang.Integer" />
    <property name="temperature" type="big_decimal" />
    <property name="weight" type="big_decimal" />
    <property name="pulse" type="java.lang.Integer" />
    <property name="breast" type="java.lang.String" />
    <property name="breastText" type="java.lang.String"
      length="200" />
    <property name="vulva" type="java.lang.String" />
    <property name="vulvaText" type="java.lang.String" length="200" />
    <property name="vagina" type="java.lang.String" />
    <property name="vaginaText" type="java.lang.String"
      length="200" />
    <property name="uterus" type="java.lang.String" />
    <property name="uterusText" type="java.lang.String"
      length="200" />
    <property name="cervix" type="java.lang.String" />
    <property name="cervixText" type="java.lang.String"
      length="200" />
    <property name="appendages" type="java.lang.String" />
    <property name="appendagesText" type="java.lang.String"
      length="200" />
    <property name="lochia" type="java.lang.String" />
    <property name="lochiaText" type="java.lang.String"
      length="200" />
    <property name="wound" type="java.lang.String" />
    <property name="woundText" type="java.lang.String" length="200" />
    <property name="healing" type="java.lang.String" />
    <property name="infectant" type="java.lang.String" />
    <property name="other" type="java.lang.String" length="200" />
    <property name="classification" type="java.lang.String"
      length="1" />
    <property name="classificationText" type="java.lang.String"
      length="200" />
    <property name="suggestion" type="java.lang.String"
      length="45" />
    <property name="suggestionText" type="java.lang.String"
      length="200" />
    <property name="treat" type="java.lang.String" length="200" />
    <property name="reason" type="java.lang.String" length="200" />
    <property name="doccol" type="java.lang.String" length="50" />
    <property name="referralNum" type="java.lang.String"
      length="16" />
    <property name="checkUnit" type="java.lang.String" length="20" />
    <property name="checkDoctor" type="java.lang.String"
      length="20" />
    <property name="checkManaUnit" type="java.lang.String" length="20"/>
    <property name="lastModifyUnit" type="java.lang.String" length="20"/>
    <property name="lastModifyDate" type="timestamp" />
    <property name="lastModifyUser" type="java.lang.String"
      length="20" />
  </class>
</hibernate-mapping>
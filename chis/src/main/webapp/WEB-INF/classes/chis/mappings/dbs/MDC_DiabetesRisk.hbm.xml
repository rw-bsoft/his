<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.dbs.schemas.MDC_DiabetesRisk"
		table="MDC_DiabetesRisk">
		<id name="riskId" type="java.lang.String">
			<column name="riskId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="phrId" type="java.lang.String" length="32" />
		<property name="fbs" type="java.lang.Double" length="8" />
		<property name="pbs" type="java.lang.Double" length="8" />
		<property name="firstAssessmentDate" type="date" length="1" />
		<property name="dataSource" type="java.lang.String" length="1" />
		<property name="effect" type="java.lang.String" length="1" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="inputDate" type="date" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="closeUnit" type="java.lang.String" length="20" />
		<property name="closeDate" type="timestamp" />
		<property name="closeUser" type="java.lang.String" length="20" />
		<property name="confirmUnit" type="java.lang.String" length="20" />
		<property name="confirmDate" type="timestamp" />
		<property name="confirmUser" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
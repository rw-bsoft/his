<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.scm.schemas.SCM_SignContractRecord" table="SCM_SignContractRecord">
		<id name="SCID" type="java.lang.Long">
			<column name="SCID"/>
			<generator class="sequence">
			<param name="sequence">SEQ_SCM_SIGNCONTRACTRECORD</param>
			</generator>
		</id>
		<property name="familyId" type="java.lang.String" length="20" />
		<property name="ownerName" type="string" length="20"/>
		<property name="scDate" type="date"  />
		<property name="beginDate" type="date"  />
		<property name="endDate" type="date"  />
		<property name="stopDate" type="date"  />
		<property name="createDate" type="timestamp"  />
		<property name="stopReason" type="java.lang.String" length="2" />
		<property name="peopleFlag" type="java.lang.String" length="2" />
		<property name="signFlag" type="java.lang.String" length="1" />
		<property name="app" type="java.lang.String" length="10" />
		<property name="favoreeEmpiId" type="java.lang.String" length="32" />
		<property name="favoreeName" type="java.lang.String" length="50" />
		<property name="favoreePhone" type="java.lang.String" length="20" />
		<property name="favoreeMobile" type="java.lang.String" length="20" />
		<property name="intendedPopulation" type="java.lang.String" length="20" />
		<property name="firstPartyName" type="java.lang.String" length="60" />
		<property name="SecondPartyEmpiId" type="java.lang.String" length="32" />
		<property name="SecondPartyName" type="java.lang.String" length="60" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="operatorUnit" type="java.lang.String" length="50" />
		<property name="operator" type="java.lang.String" length="50" />
		<property name="year" type="java.lang.String" length="1" />
		<property name="agreement" type="java.lang.String" length="10" />
		<property name="remark" type="java.lang.String" length="200" />
		<property name="signServicePackages" type="java.lang.String" length="200" />
		<property name="fphm" type="java.lang.String" length="20" />
		<property name="payOrNot" type="java.lang.String" length="5" />
	</class>
</hibernate-mapping>
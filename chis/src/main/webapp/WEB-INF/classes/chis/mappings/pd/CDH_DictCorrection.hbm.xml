<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.pd.schemas.CDH_DictCorrection" table="CDH_DictCorrection">
		<id name="recordId" type="java.lang.String">
			<column name="recordId"/>
			<generator class="assigned" />
		</id>
		<property name="diseaseType" type="java.lang.String" length="1" />
		<property name="age" type="java.lang.String" length="2" />
		<property name="suggestion" type="java.lang.String" length="4000"/>
		<property name="createUser" type="string" length="20"/>
		<property name="createUnit"  type="string" length="20"/>
		<property name="createDate" type="timestamp"/>
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.ohr.schemas.MDC_OldPeopleRecord" table="MDC_OldPeopleRecord">
		<id name="phrId" type="java.lang.String">
			<column name="phrId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="lifeSupport" type="java.lang.String" length="1" />
		<property name="abilityGrade" type="java.lang.String" length="64" />
		<property name="nurseStatus" type="java.lang.String" length="1" />
		<property name="sightStatus" type="java.lang.String" length="1" />
		<property name="toothStatus" type="java.lang.String" length="1" />
		<property name="symptomsCode" type="java.lang.String" length="2" />

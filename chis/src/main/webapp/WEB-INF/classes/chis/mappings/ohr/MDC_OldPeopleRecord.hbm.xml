		<property name="cancellationDate" type="date" />
		<property name="cancellationUnit" type="java.lang.String" length="20" />
		<property name="cancellationReason" type="java.lang.String" length="1" />
		<property name="deadReason" type="java.lang.String" length="100" />
	<!-- 增加录入信息 2019-12-12 -->
		<property name="inputUnit" type="java.lang.String" length="200" />
		<property name="inputUser" type="java.lang.String" length="20" />
	    <property name="inputDate" type="timestamp" />
	</class>
</hibernate-mapping>
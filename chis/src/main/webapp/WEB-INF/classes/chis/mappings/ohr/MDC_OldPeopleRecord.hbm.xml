		<property name="workYear" type="java.lang.Integer"  />
		<property name="protectFlag" type="java.lang.String" length="1" />
		<property name="familySmokeFlag" type="java.lang.String" length="1" />
		<property name="familyColeWarmFlag" type="java.lang.String" length="1" />

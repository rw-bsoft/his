<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.mpm.schemas.MPM_FieldMaintain" table="MPM_FieldMaintain">
			<id name="fieldId" type="java.lang.String">
            	<column name="fieldId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="alias" type="java.lang.String" length="1000" />
        	<property name="id" type="java.lang.String" length="30" />
        	<property name="type" type="java.lang.String"  length="10" />
        	<property name="dicRender" type="java.lang.String"  />
        	<property name="notNull" type="java.lang.String"  />
        	<property name="defaultValue" type="java.lang.String" length="100" />    
        	<property name="maxValue" type="java.lang.String" length="100"/> 
        	<property name="minValue" type="java.lang.String" length="100"/> 	
        	<property name="length" type="java.lang.Integer"  />
        	<property name="pyCode" type="java.lang.String" length="50" />
        	<property name="remark" type="java.lang.String"  length="100"  />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="date"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="date"  />
        	<property name="useType" type="java.lang.String" length="2" />
        	<property name="defaultType" type="java.lang.String" length="1" />
</class>
</hibernate-mapping>
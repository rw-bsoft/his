<?xml version="1.0" encoding="UTF-8"?>
<entry id="DrawOptions" alias="绘画栏" column="1" labelVisible="true">
	<item id="text" alias=" 文字" type="TextInput" >
		<css>
			<attributes width="100" text="标注文字"/>
		</css>
	 </item>
	<item id="fillColor" alias=" 颜色" type="ColorPicker" >
		<css>
			<attributes width="25" selectedColor="0xff0000"/>
		</css>
	 </item>
	 <item id="size" alias=" 大小" type="NumericStepper"> 
		<css>
			<attributes width="50" buttonMode="true" maximum="30" minimum="0" value="10"/>
		</css>
	</item>
	<item id="fillStyle" alias=" 填充" type="ComboBox"> 
		<css>
			<attributes dataProvider="solid"/>
		</css>
	</item>
	<item id="outlineStyle" alias=" 边框" type="ComboBox"> 
		<css>
			<attributes dataProvider="solid"/>
		</css>
	</item>
</entry>

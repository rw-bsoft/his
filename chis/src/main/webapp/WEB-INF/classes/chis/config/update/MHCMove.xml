    <column name="manaUnitId" param="manaUnitId"/>
    <column name="mhcDoctorId" param="mhcDoctorId"/>
    <column name="lastModifyUser" param="lastModifyUser"/>
    <column name="lastModifyUnit" param="lastModifyUnit"/>
    <column name="lastModifyDate" param="lastModifyDate"/>
  </table>
  <table name="MHC_PostnatalVisitInfo" queryField="pregnantId" queryParam="pregnantId" fullName="chis.application.mhc.schemas.MHC_PostnatalVisitInfo">
    <column name="checkManaUnit" param="manaUnitId"/>
    <column name="lastModifyUser" param="lastModifyUser"/>
    <column name="lastModifyUnit" param="lastModifyUnit"/>
    <column name="lastModifyDate" param="lastModifyDate"/>
  </table>
  <table name="MHC_Postnatal42dayRecord" queryField="pregnantId" queryParam="pregnantId" fullName="chis.application.mhc.schemas.MHC_Postnatal42dayRecord">
    <column name="checkManaUnit" param="manaUnitId"/>
    <column name="lastModifyUser" param="lastModifyUser"/>
    <column name="lastModifyUnit" param="lastModifyUnit"/>
    <column name="lastModifyDate" param="lastModifyDate"/>
  </table>
 
 
</conf>
<?xml version="1.0" encoding="UTF-8"?>
<conf id="MHCMove">
  <table name="MHC_PregnantRecord" queryField="pregnantId" queryParam="pregnantId" fullName="chis.application.mhc.schemas.MHC_PregnantRecord"><!-- 默认包含字段名字和id一致 -->
    <column name="ownerArea" param="ownerArea"/>
    <column name="homeAddress" param="homeAddress"/>
    <column name="homeAddress_text" param="homeAddress_text"/>
    <column name="restRegionCode" param="restRegionCode"/>
    <column name="restRegionCode_text" param="restRegionCode_text"/>
    <column name="residenceCode" param="residenceCode"/>
    <column name="manaUnitId" param="manaUnitId"/>
    <column name="mhcDoctorId" param="mhcDoctorId"/>
    <column name="lastModifyUser" param="lastModifyUser"/>
    <column name="lastModifyUnit" param="lastModifyUnit"/>
    <column name="lastModifyDate" param="lastModifyDate"/>
  </table> 
  <table name="MHC_WomanRecord" queryField="phrId" queryParam="phrId" fullName="chis.application.mhc.schemas.MHC_WomanRecord">

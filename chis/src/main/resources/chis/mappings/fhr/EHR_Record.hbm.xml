<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.fhr.schemas.EHR_Record" table="EHR_RECORD">
		<id name="recordMoveId" type="java.lang.String">
			<column name="recordMoveId" />
			<generator class="assigned" />
		</id>
		<property name="moveType" type="java.lang.String" length="1" />
		<property name="personName" type="java.lang.String" length="20"/>
		<property name="sexCode" type="java.lang.String" length="1"/>
		<property name="familyId" type="java.lang.String" length="20"/>
		<property name="newFamilyId" type="java.lang.String" length="20"/>
		<property name="phrId" type="java.lang.String" length="30"/>
		<property name="regionCode" type="java.lang.String" length="25" />
		<property name="regionCode_text" type="java.lang.String" length="200" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="targetArea" type="java.lang.String" length="25" />
		<property name="targetArea_text" type="java.lang.String" length="200" />
		<property name="targetUnit" type="java.lang.String" length="20"  />
		<property name="targetDoctor" type="java.lang.String" length="20" />
		<property name="applyUser" type="java.lang.String" length="20" />
		<property name="applyUnit" type="java.lang.String" length="20" />
		<property name="applyDate" type="timestamp" />
		<property name="applyReason" type="java.lang.String" length="500" />
	</class>
</hibernate-mapping>
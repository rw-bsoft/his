<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.dbs.schemas.MDC_DiabetesVisitDescription"
		table="MDC_DiabetesVisitDescription">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="visitId" type="java.lang.String" length="30" />
		<property name="tongueApprence" type="java.lang.String" />
		<property name="tongueColor" type="java.lang.String" />
		<property name="tongueMoss" type="java.lang.String" />
		<property name="color" type="java.lang.String" />
		<property name="pulse" type="java.lang.String" />
		<property name="othersCondition" type="java.lang.String" />
		<property name="differentiation" type="java.lang.String" />
		<property name="diagnosis" type="java.lang.String" />
		<property name="jianJia" type="java.lang.String" />
		<property name="guide" type="java.lang.String" />
		<property name="inputDate" type="timestamp" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String"	length="20" />
		<property name="lastModifyUser" type="java.lang.String"	length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.sch.schemas.SCH_SchistospmaRecordModule" table="SCH_SchistospmaRecord">
			<id name="schisRecordId" type="java.lang.String">
            	<column name="schisRecordId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="phrId" type="java.lang.String" length="30" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<!-- 
        	<property name="regionCode" type="java.lang.String" length="25" />
        	-->
        	<property name="manaUnitId" type="java.lang.String" length="20" />
        	<property name="checkDate" type="date"  />
        	<property name="sickenDate" type="date"  />
        	<property name="bloodCheckType" type="java.lang.String" length="1" />
        	<property name="bloodCheckResult" type="java.lang.String" length="1" />
        	<property name="checkType" type="java.lang.String" length="1" />
        	<property name="shitCheckResult" type="java.lang.String" length="1" />
        	<property name="illnessType" type="java.lang.String" length="1" />
        	<property name="findNew" type="java.lang.String" length="1" />
        	<property name="therapy" type="java.lang.String" length="1" />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="closeFlag" type="java.lang.String" length="1" />
        	<property name="closedDate" type="date" />
			<property name="closedDoctor" type="java.lang.String" length="20" />
			<property name="closedUnit" type="java.lang.String" length="20" />
			<property name="cancellationUser" type="java.lang.String"
			length="20" />
			<property name="cancellationDate" type="date" />
			<property name="cancellationUnit" type="java.lang.String"
				length="20" />
			<property name="lastModifyUnit" type="java.lang.String" length="20" />
			<property name="cancellationReason" type="java.lang.String"
				length="1" />
			<property name="deadReason" type="java.lang.String" length="100" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
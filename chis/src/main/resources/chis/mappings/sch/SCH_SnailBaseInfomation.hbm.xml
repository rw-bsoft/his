<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.sch.schemas.SCH_SnailBaseInfomation" table="SCH_SnailBaseInfomation">
			<id name="snailBaseInfoId" type="java.lang.String">
            	<column name="snailBaseInfoId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="addressId" type="java.lang.String" length="50" />
        	<property name="regionCode" type="java.lang.String" length="25" />
        	<property name="regionCode_text" type="java.lang.String" length="200" />
        	<property name="snailAddress" type="java.lang.String" length="50" />
        	<property name="environmentType" type="java.lang.String" length="1" />
        	<property name="length" type="java.lang.Double"  />
        	<property name="width" type="java.lang.Double"  />
        	<property name="area" type="java.lang.Double"  />
        	<property name="examineType" type="java.lang.String" length="1" />
        	<property name="snailPseron" type="java.lang.String" length="20" />
        	<property name="examineCount" type="java.lang.Integer"  />
        	<property name="snailFind" type="java.lang.Integer"  />
        	<property name="densityAverage" type="java.lang.Double"  />
        	<property name="densityMaximum" type="java.lang.Double"  />
        	<property name="snailNegative" type="java.lang.Integer"  />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="timestamp"  />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20"/>
        	<property name="lastModifyDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class entity-name="Referrals" table="referrals" >
    	<id name="reserveNo" type="java.lang.String"		length="16">
        	<column name="reserveNo"  />
        	<generator class="sequence">
				<param name="sequence">DR_REFERRALS_SEQ</param>
			</generator>
		</id> 
    </class>
</hibernate-mapping>

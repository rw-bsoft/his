<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.cdh.schemas.CDH_DebilityChildrenVisit"
		table="CDH_DebilityChildrenVisit">
		<id name="visitId" type="java.lang.String">
			<column name="visitId" />
			<generator class="assigned" />
		</id>
		<property name="recordId" type="java.lang.String" length="16" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="visitDate" type="date" />
		<property name="visitMonth" type="java.lang.Integer" />
		<property name="symptoms" type="java.lang.String" length="100" />
		<property name="signs" type="java.lang.String" length="100" />
		<property name="feedWay" type="java.lang.String" length="20" />	
		<property name="examination" type="java.lang.String"
			length="100" />
		<property name="results" type="java.lang.String" length="100" />
		<property name="guidance" type="text" length="1048576000" />
		<property name="appraiseWY" type="java.lang.String" length="20" />
		<property name="appraiseHY" type="java.lang.String" length="20" />
		<property name="appraiseWH" type="java.lang.String" length="20" />
		<property name="medicine" type="java.lang.String" length="500" />
		<property name="nextVisitDate" type="date" />
		<property name="examineDate" type="timestamp" />
		<property name="examineDoctor" type="java.lang.String"
			length="20" />
		<property name="examineUnit" type="java.lang.String" length="20" />

		<property name="length" type="java.lang.Double" />
		<property name="weight" type="java.lang.Double" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.def.schemas.DEF_IntellectDeformityRecordLogout"
		table="DEF_IntellectDeformityRecord">
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		
		<property name="cancellationReason" type="java.lang.String"  />
		<property name="cancellationUnit" type="java.lang.String"  />
		<property name="cancellationDate" type="date"  />
		<property name="cancellationUser" type="java.lang.String"  />
		<property name="deadReason" type="java.lang.String" length="100" />
		<property name="deadDate" type="date" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
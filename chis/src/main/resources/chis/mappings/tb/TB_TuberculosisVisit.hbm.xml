<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.tb.schemas.TB_TuberculosisVisit" table="TB_TuberculosisVisit">
		<id name="visitId" type="java.lang.String">
			<column name="visitId"/>
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="RecordId" type="java.lang.String" length="30" />
		<property name="visitDate" type="date"  />
		<property name="treatmentMonthOrder" type="java.lang.String" length="10" />
		<property name="supervisor" type="java.lang.String" length="1" />
		<property name="visitType" type="java.lang.String" length="1" />
		<property name="symptomSign" type="java.lang.String" length="50" />
		<property name="otherSymptomSign" type="java.lang.String" length="50" />
		<property name="smokePerDay" type="java.lang.String" length="10" />
		<property name="smokeDay" type="java.lang.String" length="10" />
		<property name="drinkPerDay" type="java.lang.String" length="10" />
		<property name="drinkDay" type="java.lang.String" length="10" />
		<property name="chemotherapy" type="java.lang.String" length="50" />
		<property name="medicineUsage" type="java.lang.String" length="1" />
		<property name="medicineType" type="java.lang.String" length="10" />
		<property name="missMedicineNum" type="java.lang.String" length="10" />
		<property name="medicineBadReaction" type="java.lang.String" length="1" />
		<property name="medicineBadReactionDesc" type="java.lang.String" length="100" />
		<property name="complicationComorbidity" type="java.lang.String" length="1" />
		<property name="complicationComorbidityDesc" type="java.lang.String" length="100" />
		<property name="referralDivision" type="java.lang.String" length="50" />
		<property name="referralReason" type="java.lang.String" length="100" />
		<property name="twoWeekVisitResult" type="java.lang.String" length="100" />
		<property name="processAdvice" type="java.lang.String" length="100" />
		<property name="nextDate" type="date"  />
		<property name="visitDoctor" type="java.lang.String" length="20" />
		<property name="stopTreatmentTime" type="date"  />
		<property name="stopTreatmentReason" type="java.lang.String" length="20" />
		<property name="shouldVisitNum" type="java.lang.String" length="10" />
		<property name="actualVisitNum" type="java.lang.String" length="10" />
		<property name="shouldMedicineNum" type="java.lang.String" length="10" />
		<property name="actualMedicineNum" type="java.lang.String" length="10" />
		<property name="assessDoctor" type="java.lang.String" length="20" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp"  />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp"  />
	</class>
</hibernate-mapping>
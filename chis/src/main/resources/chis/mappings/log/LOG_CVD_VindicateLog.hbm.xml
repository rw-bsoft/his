<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.log.schemas.LOG_CVD_VindicateLog" table="LOG_CVD_VindicateLog">
			<id name="logId" type="java.lang.String">
            	<column name="logId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="recordId" type="java.lang.String" length="32" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="logType" type="java.lang.String" length="10" />
        	<property name="operateType" type="java.lang.String" length="1" />
        	<property name="updateTable" type="java.lang.String" length="200" />
        	<property name="createDate" type="timestamp"  />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hy.schemas.MDC_HypertensionFirst"
		table="MDC_HypertensionFirst">
		<id name="recordNumber" type="java.lang.String">
			<column name="recordNumber" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="postHypertension" type="java.lang.String"
			length="1" />
		<property name="diagnosisType" type="java.lang.String"
			length="1" />
		<property name="diagnosisDate" type="date" />
		<property name="diagnosisDoctor" type="java.lang.String"
			length="20" />
		<property name="diagnosisUnit" type="java.lang.String"
			length="20" />
		<property name="hypertensionFirstDate" type="date" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>
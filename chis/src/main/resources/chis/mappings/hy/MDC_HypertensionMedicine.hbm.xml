<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hy.schemas.MDC_HypertensionMedicine"
		table="MDC_HypertensionMedicine">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="visitId" type="java.lang.String" length="16" />
		<property name="medicineId" type="java.lang.String" length="16" />
		<property name="medicineName" type="java.lang.String"
			length="50" />
		<property name="medicineFrequency" type="java.lang.String"
			length="20" />
		<property name="days" type="java.lang.Integer"/>
		<property name="medicineUnit" type="java.lang.String"
			length="6" />
		<property name="medicineDate" type="date" />
		<property name="totalCount" type="java.lang.Double" length="6" />
		<property name="otherMedicineDesc" type="java.lang.String"
			length="100" />
		<property name="useUnits" type="java.lang.String" length="10" />
		<property name="medicineDosage" type="java.lang.Double"
			length="10" />
		<property name="medicineTotalDosage" type="java.lang.Double"
			length="10" />
		<property name="medicineWay" type="java.lang.String" length="3" />
		<property name="medicineType" type="java.lang.String" length="2" />
		<property name="sideEffectsFlag" type="java.lang.String"
			length="1" />
		<property name="sideEffects" type="java.lang.String"
			length="100" />
		<property name="createUnit" length="20" type="java.lang.String"/>
		<property name="createDate" type="timestamp"/>
		<property name="createUser" length="20" type="java.lang.String"/>
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="timestamp" />
	</class>
</hibernate-mapping>
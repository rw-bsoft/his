<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.hy.schemas.MDC_RiskLevel" table="MDC_RiskLevel">
			<id name="levelId" type="java.lang.String">
            	<column name="levelId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="riskiness" type="java.lang.String" length="1" />
        	<property name="hypertensionLevel" type="java.lang.String" length="1" />
        	<property name="riskLevel" type="java.lang.String" length="1" />
        	<property name="riskLevelInfo" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
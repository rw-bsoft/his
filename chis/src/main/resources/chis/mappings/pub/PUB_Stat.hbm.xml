<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.pub.schemas.PUB_Stat"  table="PUB_Stat">
			<id name="statId" type="java.lang.String">
            	<column name="statId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="manaUnitId"  type="java.lang.String"   />
			<property name="healthRecord" type="java.lang.Integer"   />
			<property name="familyRecord"  type="java.lang.Integer"   />
			<property name="oldPeopleRecord"  type="java.lang.Integer"   />
			<property name="childRecord"  type="java.lang.Integer"   />
			<property name="hypertensionRecord"  type="java.lang.Integer"   />
			<property name="pregnantRecord"  type="java.lang.Integer"   />
			<property name="diabetesRecord" type="java.lang.Integer"   />
			<property name="psychosisRecord"  type="java.lang.Integer"   />
			<property name="computeDate"  type="java.lang.String"   />
			<property name="inputDate"  type="timestamp"   />
        	
</class>
</hibernate-mapping>
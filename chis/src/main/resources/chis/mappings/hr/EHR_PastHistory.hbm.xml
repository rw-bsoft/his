<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.hr.schemas.EHR_PastHistory" table="EHR_PastHistory">
		<id name="pastHistoryId" type="java.lang.String">
			<column name="pastHistoryId" />
			<generator class="assigned" />
		</id>
		<property name="empiId" type="java.lang.String" length="32" />

		<property name="pastHisTypeCode" type="java.lang.String" length="2" />
		<property name="methodsCode" type="java.lang.String" length="1" />
		<property name="codeType" type="java.lang.String" length="30" />
		<property name="protect" type="java.lang.String" length="50" />
		<property name="diseaseCode" type="java.lang.String" length="30" />
		<property name="diseaseText" type="java.lang.String" length="500" />
		<property name="confirmDate" type="date" />
		<property name="startDate" type="date" />
		<property name="endDate" type="date" />
		<property name="vestingCode" type="java.lang.String" length="8" />
		<property name="recordUser" type="java.lang.String" length="20" />
		<property name="recordUnit" type="java.lang.String" length="20" />
		<property name="recordDate" type="timestamp" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
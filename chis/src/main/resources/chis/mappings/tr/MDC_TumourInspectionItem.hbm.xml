<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.tr.schemas.MDC_TumourInspectionItem" table="MDC_TumourInspectionItem">
        	<id name="itemId" type="java.lang.String">
            	<column name="itemId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="itemCode" type="java.lang.String" length="20" />
        	<property name="itemType" type="java.lang.String" length="1" />
        	<property name="bigItemId" type="java.lang.Long" length="18" />
        	<property name="bigItemName" type="java.lang.String" length="20" />
        	<property name="definiteItemId" type="java.lang.Long" length="18" />
        	<property name="definiteItemName" type="java.lang.String" length="100" />
        	<property name="startUsing" type="java.lang.String" length="1" />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
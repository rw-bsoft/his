<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.scm.schemas.SCM_ServicePlan" table="SCM_ServicePlan">
			<id name="planId" type="java.lang.String">
            	<column name="planId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="SCID" type="java.lang.String" length="20" />
        	<property name="beginDate" type="date"  />
        	<property name="planDate" type="date"  />
        	<property name="endDate" type="date"  />
        	<property name="planStatus" type="java.lang.String" length="1" />
</class>
</hibernate-mapping>
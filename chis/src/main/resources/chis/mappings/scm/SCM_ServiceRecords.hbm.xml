<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.scm.schemas.SCM_ServiceRecords" table="SCM_ServiceRecords">
			<id name="SRID" type="java.lang.String">
            	<column name="SRID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="itemCode" type="java.lang.String" length="20" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="SCID" type="java.lang.String" length="20" />
        	<property name="planId" type="java.lang.String" length="20" />
        	<property name="taskId" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
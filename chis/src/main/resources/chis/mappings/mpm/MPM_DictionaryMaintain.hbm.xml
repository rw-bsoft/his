<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.mpm.schemas.MPM_DictionaryMaintain" table="MPM_DictionaryMaintain">
			<id name="dicId" type="java.lang.String">
            	<column name="dicId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="fieldId" type="java.lang.String" length="16" />
        	<property name="keys" type="java.lang.String" length="10" />
        	<property name="text" type="java.lang.String" length="1000" />
        	<property name="score" type="java.lang.Integer" />
        	<property name="isAnswer" type="java.lang.String"/>
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="date"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="date"  />
</class>
</hibernate-mapping>
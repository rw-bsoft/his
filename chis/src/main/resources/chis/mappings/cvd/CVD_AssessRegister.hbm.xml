<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.cvd.schemas.CVD_AssessRegister"
		table="CVD_AssessRegister">
		<id name="inquireId" type="java.lang.String">
			<column name="inquireId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="smoke" type="java.lang.String" length="1" />
		<property name="homeAddress" type="java.lang.String"
			length="50" />
		<property name="height" type="java.lang.Double" length="5" />
		<property name="weight" type="java.lang.Double" length="5" />
		<property name="waistLine" type="java.lang.Double" length="5" />
		<property name="hipLine" type="java.lang.Double" length="5" />
		<property name="tc" type="java.lang.Double" length="8" />
		<property name="ldl" type="java.lang.Double" length="8" />
		<property name="hdl" type="java.lang.Double" length="8" />
		<property name="tg" type="java.lang.Double" length="8" />
		<property name="diabetes" type="java.lang.String" length="1" />
		<property name="fbs" type="java.lang.Double" length="8" />
		<property name="pbs" type="java.lang.Double" length="8" />
		<property name="rbs" type="java.lang.Double" length="8" />
		<property name="hypertension" type="java.lang.String"
			length="1" />
		<property name="constriction" type="java.lang.Integer" />
		<property name="diastolic" type="java.lang.Integer" />
		<property name="hypertensionTreat" type="java.lang.String"
			length="1" />
		<property name="bpIncreased" type="java.lang.String" length="1" />
		<property name="cardiovascular" type="java.lang.String"
			length="128" />
		<property name="kidney" type="java.lang.String" length="1" />
		<property name="riskiness" type="java.lang.String" length="128" />
		<property name="inputDate" type="date" />
		<property name="inputUnit" type="java.lang.String" length="16" />
		<property name="inputUser" type="java.lang.String" length="16" />
		<property name="answer" type="java.lang.String" length="200" />
		<property name="lastModifyDate" type="timestamp" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="isImport" type="java.lang.String"
			length="1" />
		<property name="isDelete" type="java.lang.String"
			length="1" />
		<property name="isImported" type="java.lang.String"
			length="1" />
	</class>
</hibernate-mapping>
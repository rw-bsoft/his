<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.per.schemas.PER_CheckupBady" table="PER_CheckupBady">
			<id name="checkupNo" type="java.lang.String">
            	<column name="checkupNo"/>
            	<generator class="assigned" />
        	</id>
        	<property name="phrId" type="java.lang.String" length="30" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="guardianName" type="java.lang.String" length="20" />
        	<property name="telephone" type="java.lang.String" length="20" />
        	<property name="litters" type="java.lang.Integer"  />
        	<property name="birth" type="java.lang.Integer"  />
        	<property name="birthWeight" type="java.lang.Double" length="8" />
        	<property name="boneCondition" type="java.lang.String" length="1" />
        	<property name="boneHistory" type="java.lang.String" length="1" />
        	<property name="isAsphyxia" type="java.lang.String" length="1" />
        	<property name="asphyxiaLevel" type="java.lang.String" length="1" />
        	<property name="boneHarm" type="java.lang.String" length="1" />
        	<property name="isJaundice" type="java.lang.String" length="1" />
        	<property name="isSpasm" type="java.lang.String" length="1" />
        	<property name="happenDate" type="date"  />
        	<property name="checkupDate" type="date"  />
        	<property name="screenResult" type="java.lang.String" length="1" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="date"  />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
</class>
</hibernate-mapping>
			</id>
        	<property name="checkupNo" type="java.lang.String" length="16" />
        	<property name="recordNo" type="java.lang.String" length="30" />
        	<property name="empiId" type="java.lang.String" length="32" />
        	<property name="immunityName" type="java.lang.String" length="100" />
        	<property name="immunityDate" type="java.lang.String" length="10" />
        	<property name="immunityUnit" type="java.lang.String" length="100" />
</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.psy.schemas.PSY_AnnualAssessment" table="PSY_AnnualAssessment">
		<id name="assessmentId" type="java.lang.String">
			<column name="assessmentId" />
			<generator class="assigned" />
		</id>
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="treamentYN" type="java.lang.String" length="1" />
		<property name="overallCondition" type="java.lang.String" length="1" />
		<property name="workAbility" type="java.lang.String" length="1" />
		<property name="personalLife" type="java.lang.String" length="1" />
		<property name="housework" type="java.lang.String" length="1" />
		<property name="produce" type="java.lang.String" length="1" />
		<property name="learningAbility" type="java.lang.String" length="1" />
		<property name="communication" type="java.lang.String" length="1" />
		<property name="overallAssessment" type="java.lang.String" length="1" />
		<property name="suggest" type="java.lang.String" length="2000" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createDate" type="timestamp" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.her.schemas.HER_EducationPlanSet" table="HER_EducationPlanSet">
			<id name="setId" type="java.lang.String">
            	<column name="setId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="planType" type="java.lang.String" length="50" />
        	<property name="beginDate" type="date"  />
        	<property name="endDate" type="date"  />
        	<property name="executePerson" type="java.lang.String" length="1000" />
        	<property name="planContent" type="java.lang.String" length="1000" />
        	<property name="executeResult" type="java.lang.String" length="100" />
        	<property name="planPerson" type="java.lang.String" length="20" />
        	<property name="planDate" type="timestamp"  />
        	<property name="planUnit" type="java.lang.String" length="20" />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
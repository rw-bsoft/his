<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.gdr.schemas.GDR_Visit" table="GDR_Visit">
			<id name="visitId" type="java.lang.String">
            	<column name="visitId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="gdrId" type="java.lang.String" length="16" />
        	<property name="poisoning" type="java.lang.String" length="1" />
        	<property name="poisoningCount" type="java.lang.Integer"  />
        	<property name="poisoningDesc" type="java.lang.String" length="200" />
        	<property name="visitUser" type="java.lang.String" length="20" />
        	<property name="visitUnit" type="java.lang.String" length="20" />
        	<property name="visitDate" type="date"  />
        	<property name="createUser" type="java.lang.String" length="20" />
        	<property name="createUnit" type="java.lang.String" length="20" />
        	<property name="createDate" type="timestamp"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="timestamp"  />
</class>
</hibernate-mapping>
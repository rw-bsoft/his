<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="ZY_LJFY" table="ZY_LJFY">
		<composite-id>
			<key-property name="JGID" column="JGID" type="java.lang.String"/>
			<key-property name="FYXH" column="FYXH" type="java.lang.Long" />
		</composite-id>
		<property name="FYDJ" type="java.lang.Double" length="8" />
		<property name="KSRQ" type="timestamp"  />
		<property name="JSRQ" type="timestamp"  />
		<property name="LJFS" type="java.lang.Integer" length="2" />
		<property name="XZFS" type="java.lang.Integer" length="2" />
	</class>
</hibernate-mapping>
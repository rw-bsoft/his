<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="PUB_DrugDirectory" table="PUB_DrugDirectory">
		<id name="drugCode" type="java.lang.String">
			<column name="drugCode" />
			<generator class="assigned" />
		</id>
		<property name="drugName" type="java.lang.String" length="80" />
		<property name="drugspec" type="java.lang.String" length="50" />
		<property name="drugunits" type="java.lang.String" length="6" />
		<property name="minunits" type="java.lang.String" length="6" />
		<property name="packnum" type="java.lang.Integer" />
		<property name="drugtype" type="java.lang.String" length="1" />
		<property name="classcode" type="java.lang.String" length="30" />
		<property name="formulation" type="java.lang.String" length="20" />
		<property name="pycode" type="java.lang.String" length="20" />
		<property name="ctflag" type="java.lang.String" length="1" />
		<property name="usemode" type="java.lang.String" length="2" />
		<property name="invalid" type="java.lang.String" length="1" />
		<property name="drugtext" type="java.lang.String" length="4000" />
		<property name="enName"	type="java.lang.String" length="100"/>
		<property name="mdcUse" type="java.lang.String" length="1"/>
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="phis.application.cic.schemas.PUB_PelpleHealthTeach"
		table="PUB_PelpleHealthTeach">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>
		<property name="recipeName" type="java.lang.String" length="200" />
		<property name="diagnoseName" type="java.lang.String" length="200" />
		<property name="recipeNamePy" type="java.lang.String" length="200" />
		<property name="diagnoseNamePy" type="java.lang.String" length="200" />
		<property name="ICD10" type="java.lang.String" length="50" />
		<property name="healthTeach" type="java.lang.String" length="4000" />
		<property name="inputDate" type="date" />
		<property name="inputUser" type="java.lang.String" length="20" />
		<property name="inputUnit" type="java.lang.String" length="20" />
		<property name="lastModifyDate" type="date" />
		<property name="lastModifyUser" type="java.lang.String"
			length="20" />
		<property name="lastModifyUnit" type="java.lang.String"
			length="20" />
	</class>
</hibernate-mapping>
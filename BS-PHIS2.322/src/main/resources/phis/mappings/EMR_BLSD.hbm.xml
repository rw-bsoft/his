<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="EMR_BLSD" table="EMR_BLSD">
		<id name="BLBH" type="java.lang.Long">
			<column name="BLBH"/>
			<generator class="assigned" />
		</id>
		<property name="SDYG" type="java.lang.String" length="10" />
		<property name="SDIP" type="java.lang.String" length="15" />
		<property name="SDSJ" type="timestamp" />
		<property name="SDZT" type="java.lang.Integer"   length="1"/>
		<property name="JGID" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>
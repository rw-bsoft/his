<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.scm.schemas.SCM_ServicePackageItems" table="SCM_ServicePackageItems">
			<id name="SPIID" type="java.lang.String">
            	<column name="SPIID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="SPID" type="java.lang.String" length="20" />
        	<property name="itemCode" type="java.lang.String" length="20" />
        	<property name="SERVICETIMES" type="java.lang.Integer" />
        	<property name="status" type="java.lang.String" length="1" />
        	<property name="LOGOFF" type="java.lang.Integer" length="1"/>
			<property name="sortNumberSPI" type="java.lang.Integer" length="3" />   	
        	<property name="ztmc" type="java.lang.String" length="50" />
</class>
</hibernate-mapping>
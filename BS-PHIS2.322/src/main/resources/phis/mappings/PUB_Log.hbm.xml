<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="PUB_Log" table="PUB_Log">
		<id name="recordId" type="java.lang.String">
			<column name="recordId" />
			<generator class="assigned" />
		</id>

		<property name="operType" type="java.lang.String" length="1" />
		<property name="recordType" type="java.lang.String" length="2" />
		<property name="phrId" type="java.lang.String" length="30" />
		<property name="empiId" type="java.lang.String" length="32" />
		<property name="personName" type="java.lang.String" length="16" />
		<property name="idCard" type="java.lang.String" length="20" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		<property name="regionCode" type="java.lang.String" length="25" />
		<property name="regionCode_text" type="java.lang.String" length="200" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="description" type="java.lang.String" length="2000" />
		<property name="createUnit" type="java.lang.String" length="20" />
		<property name="createUser" type="java.lang.String" length="20" />
		<property name="createTime" type="date" />
	</class>
</hibernate-mapping>
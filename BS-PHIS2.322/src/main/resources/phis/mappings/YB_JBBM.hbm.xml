<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="YB_JBBM" table="YB_JBBM">
	<!--医保卡信息表,其他的字段根据各地医保卡返回值添加-->
			<id name="JBBM" type="java.lang.String">
            	<column name="JBBM"/>
            	<generator class="assigned" />
        	</id>
        	<property name="JBMC" type="java.lang.String" length="40" />
        	<property name="PYDM" type="java.lang.String" length="9" />
        	<property name="ZFPB" type="java.lang.Integer" length="1" />
</class>
</hibernate-mapping>